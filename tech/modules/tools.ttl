tech:FileSystem a rdfs:Class,
        skos:Concept,
        dpv:Technology ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2022-06-15"^^xsd:date ;
    rdfs:isDefinedBy tech: ;
    sw:term_status "accepted"@en ;
