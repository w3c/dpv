    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "State where a request for consent has been made and is awaiting a decision"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Consent Requested"
      }
