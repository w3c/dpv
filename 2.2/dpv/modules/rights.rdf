    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples#E0057"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#rights-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#RightNonFulfilmentNotice">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#OrganisationalMeasure"/>
    <skos:prefLabel xml:lang="en">Right Non-Fulfilment Notice</skos:prefLabel>
    <skos:definition xml:lang="en">Notice provided regarding non-fulfilment of a right</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#RightExerciseNotice"/>
    <skos:scopeNote xml:lang="en">This notice is associated with situations where information is provided with the intention of communicating non-fulfilment of a right. For example, to provide justifications on why a right could not be fulfilled or providing information about another entity who should be approached for exercising this right.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-02</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
