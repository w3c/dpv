    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples#E0059"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#rights-classes"/>
  </rdf:Description>
