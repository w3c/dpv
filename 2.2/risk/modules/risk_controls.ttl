risk:AvoidanceControl a rdfs:Class,
        skos:Concept,
        risk:RiskControl ;
    dct:contributor _:person-harshvardhan-j-pandit ;
    dct:created "2025-01-06"^^xsd:date ;
    rdfs:isDefinedBy risk: ;
    sw:term_status "accepted"@en ;
    skos:broader risk:ProactiveControl ;
    skos:definition "Control that avoids an event with the goal of removing it completely"@en ;
    skos:inScheme risk:risk-controls-classes ;
    skos:prefLabel "Avoidance Control"@en ;
    skos:scopeNote "Avoiding is distinct from Mitigation and Modification as the goal to avoid an event is to prevent it from occurring at all, whereas mitigation and modification accept an event will occur and focus on managing it"@en .

