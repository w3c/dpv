      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Control that eliminates an event entirely such that the event does not occur"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://w3id.org/dpv/risk#risk-controls-classes"
