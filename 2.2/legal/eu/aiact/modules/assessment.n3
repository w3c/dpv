eu-aiact:AIRegulatorySandbox a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(55))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:RegulatorySandbox ;
    skos:definition "A controlled framework set up by a competent authority which offers providers or prospective providers of AI systems the possibility to develop, train, validate and test, where appropriate in real-world conditions, an innovative AI system, pursuant to a sandbox plan for a limited time under regulatory supervision"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "AI Regulatory Sandbox"@en .

eu-aiact:AISystemPerformance a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(18))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "The ability of an AI system to achieve its intended purpose"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "AI System Performance"@en .

eu-aiact:CEMarking a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA(Art. 3(24))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:CertificationSeal ;
    skos:definition "A marking by which a provider indicates that an AI system is in conformity with the requirements set out in Chapter III, Section 2 and other applicable Union harmonisation legislation providing for its affixing"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "CE Marking"@en .

eu-aiact:ConformityAssessment a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA(Art. 3(20))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:Assessment ;
    skos:definition "The process of demonstrating whether the requirements set out in Chapter III, Section 2 relating to a high-risk AI system have been fulfilled"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Conformity Assessment"@en .

eu-aiact:FRIA a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA(Art. 27)"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:Assessment ;
    skos:definition "An assessment undertaken to evaluate how the AI use of an AI system might impact fundamental rights"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Fundamental Rights Impact Assessment"@en .

eu-aiact:HighRiskAIAssessment a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA(Art. 6)"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:Assessment ;
    skos:definition "An assessment undertaken to determine whether the AI system is classified as high-risk under the AI Act"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "High Risk AI Assessment"@en .

eu-aiact:InformedConsent a rdfs:Class,
        skos:Concept,
        dpv:LegalBasis ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(59))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:InformedConsent ;
    skos:definition "Informed consent means a (real world test) subject’s freely given, specific, unambiguous and voluntary expression of his or her willingness to participate in a particular testing in real world conditions, after having been informed of all aspects of the testing that are relevant to the subject’s decision to participate"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Informed Consent"@en .

eu-aiact:PostMarketMonitoringSystem a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(25))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:OrganisationalMeasure ;
    skos:definition """All activities carried out by providers of AI systems to collect and review experience gained from the use of AI systems they
place on the market or put into service for the purpose of identifying any need to immediately apply any necessary corrective or preventive actions"""@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Post-Market Monitoring System"@en .

eu-aiact:RealWorldTestSubject a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(58))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf dpv:NaturalPerson ;
    sw:term_status "modified"@en ;
    skos:broader dpv:NaturalPerson ;
    skos:definition "A natural person who participates in testing in real world conditions"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Real World Test Subject"@en ;
    skos:scopeNote "Note: The term \"Real World Test Subject\" is defined as just \"Subject\" in the AI Act. We emphasise its distinction from other subjects, e.g. AI Subject, with this phrasing of the term."@en .

eu-aiact:TestingRealWorldConditions a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2024-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(57))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:OrganisationalMeasure ;
    skos:definition "The temporary testing of an AI system for its intended purpose in real-world conditions outside a laboratory or otherwise simulated environment, with a view to gathering reliable and robust data and to assessing and verifying the conformity of the AI system with the requirements of the AI Act and it does not qualify as placing the AI system on the market or putting it into service within the meaning of the AI Act, provided that all the conditions laid down in Article 57 or 60 are fulfilled"@en ;
    skos:inScheme eu-aiact:assessment-classes ;
    skos:prefLabel "Testing In Real World Conditions"@en .

