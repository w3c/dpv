eu-aiact-owl:BiometricCategorisation a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art. 3(40)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:BiometricCategorisation ;
    sw:term_status "modified"@en ;
    skos:definition "Capability where assigning natural persons to specific categories happens on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons"@en ;
    skos:prefLabel "Biometric Categorisation"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:BiometricIdentification a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art. 3(35)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:BiometricIdentification ;
    sw:term_status "modified"@en ;
    skos:definition "Capability where the automated recognition of physical, physiological, behavioural, and psychological human features are used for the purpose of establishing the identity of a natural person by comparing biometric data of that individual to to biometric data of individuals stored in a database"@en ;
    skos:prefLabel "Biometric Identification"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:BiometricVerification a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art.3 (36)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:IdentityVerification ;
    sw:term_status "modified"@en ;
    skos:definition "The automated, one-to-one verification, including authentication, of the identity of natural persons by comparing their biometric data to previously provided biometric data"@en ;
    skos:prefLabel "Biometric Verification"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:DeepFakeGeneration a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art. 3(60)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:BiometricIdentification ;
    sw:term_status "modified"@en ;
    skos:definition "Capability to generate or manipulate image, audio or video content that resembles existing persons, objects, places, entities or events and would falsely appear to a person to be authentic or truthful;l"@en ;
    skos:prefLabel "Deep Fake Generation"@en .

eu-aiact-owl:EmotionRecognition a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art. 3(39)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:EmotionRecognition ;
    sw:term_status "modified"@en ;
    skos:definition "Capability of a technology to identify or infer emotions or intentions of natural persons (on the basis of their biometric data)"@en ;
    skos:prefLabel "Emotion Recognition"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:HighImpactCapabilityGPAIModel a rdfs:Class,
        owl:Class,
        tech-owl:Capability ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA Art. 3(64)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "modified"@en ;
    skos:definition "‘High-impact Capabilities’ in general purpose AI models means capabilities that match or exceed the capabilities recorded in the most  advanced general purpose AI models"@en ;
    skos:prefLabel "High-Impact Capability in General Purpose AI Models"@en .

