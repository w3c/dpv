eu-nis2:EarlyWarningReport a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:modified "2025-08-05"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(4)(a)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "modified"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Report sent within 24 hours of detection containing cause of the incident and whether it was unlawful or malicious and whether there is cross-border impact"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Early Warning Report"@en .

eu-nis2:FinalReport a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:modified "2025-08-05"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(4)(d)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "modified"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Report sent within 1 month of incident handling i.e. completing the incident recovery and containing the applied/ongoing measures, and containing a 'detailed description' of the incident including its severity and impact, threat type and root cause that are likely to have triggered the incident, applied and ongoing measures, and - if applicable - cross-border impacts"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Final Report"@en .

eu-nis2:IncidentAssessmentReport a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:modified "2025-08-05"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(4)(b)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "modified"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Report sent within 72 hours of detection containing updates on the earlier information as well as initial assessment of severity and impact of the incident as well as any 'indicators of compromise'"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Incident Assessment Report"@en .

eu-nis2:InitialFeedbackOnIncident a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(5)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "accepted"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Notification from authority to organisation (upon request, within 24 hours or early warning) containing \"initial feedback\" and guidelines on measures that can be taken in response to a breach"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Initial Feedback on Incident"@en .

eu-nis2:IntermediateReport a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:modified "2025-08-05"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(4)(c)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "modified"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Report sent upon request which provides updates, if any, to previous information"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Intermediate Report"@en .

eu-nis2:ProgressReport a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:modified "2025-08-05"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(4)(e)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "modified"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Report sent within 1 month of detection if the incident handling has not been completed by then, with updates to previous information"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Progress Report"@en .

eu-nis2:RiskMitigationAdvice a rdfs:Class,
        skos:Concept,
        risk:IncidentNotice ;
    dct:contributor _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(5)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    sw:term_status "accepted"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Notification from organisation to stakeholders regarding risk mitigations to be applied and existence of threats"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Risk Mitigation Advice"@en .

eu-nis2:SignificantIncidentNotice a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "NIS 2 Directive Article 23(3)"@en ;
    rdfs:isDefinedBy eu-nis2: ;
    rdfs:subClassOf risk:IncidentNotice ;
    sw:term_status "accepted"@en ;
    skos:broader risk:IncidentNotice ;
    skos:definition "Notice sent for reporting significant incidents"@en ;
    skos:inScheme eu-nis2:notice-classes ;
    skos:prefLabel "Significant Incident Notice"@en .

