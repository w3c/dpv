  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#A46-2-e">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
