  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/gdpr/owl#BindingCorporateRules">
    <dct:source rdf:nodeID="N4a7c20841b3740728e52a001fec918c3"/>
    <skos:prefLabel xml:lang="en">Binding Corporate Rules (BCR)</skos:prefLabel>
