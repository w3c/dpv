  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#SafetyComponent">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Safety Component</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">A component of a product or of a system which fulfils a safety function for that product or system, or the failure or malfunctioning of which endangers the health and safety of persons or property</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/tech/owl#Component"/>
    <dct:source xml:lang="en">AIA (Art. 3(14))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#GPAISystem">
    <skos:definition xml:lang="en">An AI system which is based on a general-purpose AI model, that has the capability to serve a variety of purposes, both for direct use as well as for integration in other AI systems</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#AISystem"/>
    <dct:source xml:lang="en">AIA(Art.3 (44e))</dct:source>
    <skos:prefLabel xml:lang="en">General Purpose AI System</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#serialisation-rdf">
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/eu-aiact-owl.rdf"/>
    <dct:title>EU AI Act - RDF/XML serialiation</dct:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/application/rdf+xml"/>
