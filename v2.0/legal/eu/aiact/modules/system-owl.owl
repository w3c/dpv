  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#RemoteBiometricIdentificationSystem">
    <skos:related rdf:resource="https://w3id.org/dpv/v2.0/pd/owl#Biometric"/>
    <dct:source xml:lang="en">AIAArt. 3(36))</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <skos:prefLabel xml:lang="en">Remote Biometric Identification System</skos:prefLabel>
    <skos:definition xml:lang="en">An AI system for the purpose of identifying natural persons, without their active involvement, typically at a distance through the comparison of a person’s biometric data with the biometric data contained in a reference database.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#AISystem"/>
  </rdf:Description>
