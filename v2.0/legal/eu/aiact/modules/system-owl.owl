  <rdf:Description rdf:about="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#RealtimeRemoteBiometriIdentificationSystem">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA(Art. 3(37))</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:prefLabel xml:lang="en">Real-time Remote Biometric Identification System</skos:prefLabel>
    <skos:related rdf:resource="https://w3id.org/dpv/v2.0/pd/owl#Biometric"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">A remote biometric identification system whereby the capturing of biometric data, the comparison and the identification all occur without a significant delay. This comprises not only instant identification, but also limited short delays in order to avoid circumvention.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/v2.0/legal/eu/aiact/owl#RemoteBiometricIdentificationSystem"/>
  </rdf:Description>
