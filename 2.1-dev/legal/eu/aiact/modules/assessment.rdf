  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#RealWorldTestSubject">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Real World Test Subject</skos:prefLabel>
    <skos:definition xml:lang="en">A natural person who participates in testing in real world conditions</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#NaturalPerson"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#NaturalPerson"/>
    <dct:source xml:lang="en">AIA (Art. 3(bj))</dct:source>
    <skos:scopeNote xml:lang="en">Note: The term "Real World Test Subject" is defined as just "Subject" in the AI Act. We emphasise its distinction from other subjects, e.g. AI Subject, with this phrasing of the term.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#assessment-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#ConformityAssessment">
