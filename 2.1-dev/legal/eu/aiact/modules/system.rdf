    <skos:prefLabel xml:lang="en">Emotion Recognition System</skos:prefLabel>
    <skos:definition xml:lang="en">An AI system for the purpose of identifying or inferring emotions or intentions of natural persons on the basis of their biometric data.</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/aiact#AISystem"/>
    <skos:related rdf:resource="https://w3id.org/dpv/legal/eu/aiact#EmotionRecognition"/>
    <skos:related rdf:resource="https://w3id.org/dpv/pd#Biometric"/>
    <dct:source xml:lang="en">AIA(Art. 3(34))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
