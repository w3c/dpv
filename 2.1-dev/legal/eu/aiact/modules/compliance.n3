eu-aiact:AIActComplianceUnknown a rdfs:Class,
        skos:Concept,
        dpv:Lawfulness ;
    dct:contributor "Harshvardhan J. Pandit, Beatriz Esteves" ;
    dct:created "2024-07-21"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader eu-aiact:AIActLawfulness ;
