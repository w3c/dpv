    <skos:prefLabel xml:lang="en">NIS2 Lawfulness</skos:prefLabel>
    <skos:definition xml:lang="en">Status or state associated with being lawful or legally compliant regarding NIS2</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Lawfulness"/>
