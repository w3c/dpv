    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/nis2#notice-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/nis2#NIS2Lawfulness">
