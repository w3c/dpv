    rdfs:subClassOf justifications-owl:LegalityLackingObjection ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten is being exercised as the corresponding consent has been withdrawn and there is no other legal basis for the processing"@en ;
    skos:prefLabel "Justification A17 - No Legal Basis"@en .

eu-gdpr-owl:JustificationA17NonNecessity a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:NonNecessityObjection ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten is being exercised as the personal data are no longer necessary in relation to the purposes for which they were collected or otherwise processed"@en ;
    skos:prefLabel "Justification A17 - Non-Necessity"@en .

eu-gdpr-owl:JustificationA17ObjectA21 a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:Objection ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten is being exercised through A21 right to object where there are no overriding legitimate grounds for the processing (A21-1) or as an objection to direct marketing (A21-2)"@en ;
    skos:prefLabel "Justification A17 - Object A21"@en .

eu-gdpr-owl:JustificationA17OfficialAuthority a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:OfficialAuthorityExerciseImpaired ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten could not be completed due to"@en ;
    skos:prefLabel "Justification A17 - Official Authority"@en .

eu-gdpr-owl:JustificationA17PublicHealth a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:PublicHealthCompromised ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten could not be completed due to"@en ;
    skos:prefLabel "Justification A17 - Public Health"@en .

eu-gdpr-owl:JustificationA17PublicInterest a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:PublicInterestCompromised ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten could not be completed due to"@en ;
    skos:prefLabel "Justification A17 - Public Interest"@en .

eu-gdpr-owl:JustificationA17UnlawfulProcessing a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.17-"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:UnlawfulActivityObjection ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the A17 right to erasure or to be forgotten is being exercised as the personal data have been unlawfully processed"@en ;
    skos:prefLabel "Justification A17 - Unlawful Processing"@en .

eu-gdpr-owl:JustificationA33BreachedDataIneffective a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Article33(3a)"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:ProcessSafeguarded,
        eu-gdpr-owl:DataBreachJustification ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the personal data breach was not communicated to the data subject as the controller has implemented appropriate technical and organisational protection measures, and those measures were applied to the personal data affected by the personal data breach, in particular those that render the personal data unintelligible to any person who is not authorised to access it, such as encryption i.e. the breached data cannot be effectively used"@en ;
    skos:prefLabel "Justification A33 - Breached Data Ineffective"@en .

eu-gdpr-owl:JustificationA33DisproportionateEffort a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Article33(3c)"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:DisproportionateEffortRequired,
        eu-gdpr-owl:DataBreachJustification ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the personal data breach was not communicated to the data subject as it would involve disproportionate effort, and that a public communication or similar measure whereby the data subjects are informed in an equally effective manner has been deployed"@en ;
    skos:prefLabel "Justification A33 - Disproportionate Effort"@en .

eu-gdpr-owl:JustificationA33NotificationDelay a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Article33(1)"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:DelayJustification,
        eu-gdpr-owl:DataBreachJustification ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification for why the notification about personal data breach to the authority was not communicated within 72 hours after having become aware of it"@en ;
    skos:prefLabel "Justification A33 - Notification Delay"@en .

eu-gdpr-owl:JustificationA33RiskMitigated a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Article33(3b)"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:RiskMitigated,
        eu-gdpr-owl:DataBreachJustification ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the personal data breach was not communicated to the data subject as the controller has taken subsequent measures which ensure that the high risk to the rights and freedoms of data subjects is no longer likely to materialise"@en ;
    skos:prefLabel "Justification A33 - Risk Mitigated"@en .

eu-gdpr-owl:JustificationA33RiskUnlikely a rdfs:Class,
        owl:Class,
        dpv-owl:Justification ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Article33(1)"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf justifications-owl:RightsFreedomsImpactUnlikely,
        eu-gdpr-owl:DataBreachJustification ;
    sw:term_status "accepted"@en ;
    skos:definition "Justification that the notification about personal data breach was not communicated to the authority as it is unlikely to result in a risk to the rights and freedoms of natural persons"@en ;
    skos:prefLabel "Justification A33 - Risk Unlikely"@en .

eu-gdpr-owl:LawfulnessPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1a"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed processed in a lawful manner in relation to the data subject"@en ;
    skos:prefLabel "Lawfulness Principle"@en .

eu-gdpr-owl:LeadSupervisoryAuthority a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Georg P. Krog" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:definition "Authority with the primary responsibility for dealing with a cross-border data processing activity"@en ;
    skos:prefLabel "Lead Supervisory Authority"@en .

eu-gdpr-owl:LocalSupervisoryAuthority a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Georg P. Krog" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:definition "Authority associated with the main or local establishment of an organisation"@en ;
    skos:prefLabel "Local Supervisory Authority"@en .

eu-gdpr-owl:MainEstablishment a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Georg P. Krog" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " GDPR Art.56"@en,
        " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.4-16"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:Establishment ;
    sw:term_status "accepted"@en ;
    skos:definition "Main Establishment' means as regards a controller with establishments in more than one Member State, the place of its central administration in the Union, unless the decisions on the purposes and means of the processing of personal data are taken in another establishment of the controller in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment; as regards a processor with establishments in more than one Member State, the place of its central administration in the Union, or, if the processor has no central administration in the Union, the establishment of the processor in the Union where the main processing activities in the context of the activities of an establishment of the processor take place to the extent that the processor is subject to specific obligations under this Regulation;"@en ;
    skos:prefLabel "Main Establishment"@en .

eu-gdpr-owl:PersonalData a rdfs:Class,
        owl:Class,
        dpv-owl:PersonalData ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    rdf:value "sc" ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:PersonalData ;
    sw:term_status "accepted"@en ;
    skos:definition "Personal Data' means any information relating to an identified or identifiable natural person (‘Data Subject’)"@en ;
    skos:prefLabel "Personal Data"@en .

eu-gdpr-owl:Processing a rdfs:Class,
        owl:Class,
        dpv-owl:Processing ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    rdf:value "sc" ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Processing ;
    sw:term_status "accepted"@en ;
    skos:definition "‘Processing’ means any operation or set of operations which is performed on personal data or on sets of personal data, whether or not by automated means, such as collection, recording, organisation, structuring, storage, adaptation or alteration, retrieval, consultation, use, disclosure by transmission, dissemination or otherwise making available, alignment or combination, restriction, erasure or destruction"@en ;
    skos:prefLabel "Processing"@en .

eu-gdpr-owl:Processor a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-8"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:DataProcessor ;
    sw:term_status "accepted"@en ;
    skos:definition "‘Processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller"@en ;
    skos:prefLabel "Processor"@en .

eu-gdpr-owl:ProcessorBreachNotice a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:DataBreachNotice ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "GDPR Article 33"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataBreachNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "Notice regarding a data breach to the Processor"@en ;
    skos:prefLabel "Processor Breach Notice"@en .

eu-gdpr-owl:ProcessorBreachNotificationNeeded a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:DataBreachNoticeRequirement ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "GDPR Article 33"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataBreachNoticeRequirement ;
    sw:term_status "accepted"@en ;
    skos:definition "Data Breach notification to the Processor is required"@en ;
    skos:prefLabel "Processor Breach Notification Needed"@en .

eu-gdpr-owl:Profiling a rdfs:Class,
        owl:Class,
        dpv-owl:Profiling ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    rdf:value "eu-gdpr:Processing" ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Profiling ;
    sw:term_status "accepted"@en ;
    skos:definition "‘Profiling’ means any form of automated processing of personal data consisting of the use of personal data to evaluate certain personal aspects relating to a natural person, in particular to analyse or predict aspects concerning that natural person’s performance at work, economic situation, health, personal preferences, interests, reliability, behaviour, location or movements"@en ;
    skos:prefLabel "Profiling"@en .

eu-gdpr-owl:Pseudonymisation a rdfs:Class,
        owl:Class,
        dpv-owl:Pseudonymisation ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    rdf:value "dpv:TechnicalMeasure" ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Pseudonymisation ;
    sw:term_status "accepted"@en ;
    skos:definition "Pseudonymisation’ means the processing of personal data in such a manner that the personal data can no longer be attributed to a specific data subject without the use of additional information, provided that such additional information is kept separately and is subject to technical and organisational measures to ensure that the personal data are not attributed to an identified or identifiable natural person"@en ;
    skos:prefLabel "Pseudonymisation"@en .

eu-gdpr-owl:PurposeLimitationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1b"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes; further processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes shall, in accordance with Article 89(1), not be considered to be incompatible with the initial purposes"@en ;
    skos:prefLabel "Purpose Limitation Principle"@en .

eu-gdpr-owl:Recipient a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-9"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Recipient ;
    sw:term_status "accepted"@en ;
    skos:definition "‘Recipient’ means a natural or legal person, public authority, agency or another body, to which the personal data are disclosed, whether a third party or not"@en ;
    skos:prefLabel "Recipient"@en .

eu-gdpr-owl:Representative a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-17"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Representative ;
    sw:term_status "accepted"@en ;
    skos:definition "Representative’ means a natural or legal person established in the Union who, designated by the controller or processor in writing pursuant to Article 27, represents the controller or processor with regard to their respective obligations under this Regulation"@en ;
    skos:prefLabel "Representative"@en .

eu-gdpr-owl:RightsRecipientsNotice a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2022-11-09"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:RightFulfilmentNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "A Notice provided in fulfilment of GDPR's Art.19 regarding Recipients to whom a rights exercise has been communicated, such as regarding rectification (A.16) or erasure of personal data (A.17) or restriction of processing (A.18)"@en ;
    skos:prefLabel "Rights Recipients Notice"@en .

eu-gdpr-owl:SARNotice a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2022-11-09"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:RightFulfilmentNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "A Notice provided in fulfilment of GDPR's Art.15 regarding information to be provided for Right of Access or Subject Access Request (SAR)"@en ;
    skos:prefLabel "SAR Notice"@en .

eu-gdpr-owl:SCCByCommission a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "GDPR Art.46-2c" ;
            schema:url "https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj" ] ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataTransferTool,
        eu-gdpr-owl:StandardContractualClauses ;
    sw:term_status "accepted"@en ;
    skos:definition "Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)"@en ;
    skos:prefLabel "SCCs adopted by Commission"@en .

eu-gdpr-owl:SCCBySupervisoryAuthority a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "GDPR Art.46-2d" ;
            schema:url "https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_d/oj" ] ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataTransferTool,
        eu-gdpr-owl:StandardContractualClauses ;
    sw:term_status "accepted"@en ;
    skos:definition "Standard data protection clauses adopted by a supervisory authority and approved by the Commission pursuant to the examination procedure referred to in GDPR Article 93(2)"@en ;
    skos:prefLabel "SCCs adopted by Supervisory Authority"@en .

eu-gdpr-owl:SingleEstablishment a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Georg P. Krog" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source "GDPR Art.23"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:Establishment ;
    sw:term_status "accepted"@en ;
    skos:definition "A legal entity that is established in only one Member State"@en ;
    skos:prefLabel "Single Establishment"@en .

eu-gdpr-owl:StandardContractualClauses a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "Implementing Decision on SCC for Data Transfers" ;
            schema:url "https://eur-lex.europa.eu/eli/dec_impl/2021/914/oj" ] ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataTransferTool,
        dpv-owl:Contract ;
    sw:term_status "accepted"@en ;
    skos:definition "Standard Contractual Clauses (SCCs) are pre-approved clauses by the EU for ensuring appropriate data protection safeguards intended for data transfers from the EU to third countries"@en ;
    skos:prefLabel "Standard Contractual Clauses (SCC)"@en .

eu-gdpr-owl:StorageLimitationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1e"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be kept in a form which permits identification of data subjects for no longer than is necessary for the purposes for which the personal data are processed; personal data may be stored for longer periods insofar as the personal data will be processed solely for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes in accordance with Article 89(1) subject to implementation of the appropriate technical and organisational measures required by this Regulation in order to safeguard the rights and freedoms of the data subject"@en ;
    skos:prefLabel "Storage Limitation Principle"@en .

eu-gdpr-owl:SupplementaryMeasure a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "EDPB Recommendations 01/2020 on Supplementary Measures and Transfer Tools" ;
            schema:url "https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf" ] ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataTransferTool ;
    sw:term_status "accepted"@en ;
    skos:definition "Supplementary measures are intended to additionally provide safeguards or guarantees to bring the resulting protection in line with EU requirements"@en ;
    skos:prefLabel "Supplementary Measure"@en .

eu-gdpr-owl:ThirdParty a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-10"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:ThirdParty ;
    sw:term_status "accepted"@en ;
    skos:definition "‘Third Party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data"@en ;
    skos:prefLabel "Third Party"@en .

eu-gdpr-owl:TransparencyPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1a"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data must be processed processed in a transparent manner in relation to the data subject"@en ;
    skos:prefLabel "Transparency Principle"@en .

dct:conformsTo a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:conformsTo"@en ;
    skos:scopeNote "For expressing an existing standard, guideline, or requirements to which the DPIA document or process will be conforming to. This could be external guidelines published by an Authority, or internal guidelines established by the organisation"@en .

dct:coverage a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:coverage"@en ;
    skos:scopeNote "For expressing coverage (e.g. jurisdictions, products, services) of the DPIA document or process. For temporal coverage, please see dct:temporal. The coverage can be expressed using dpv:Process, or using another concept, or even be a link or reference to a document, or a textual description"@en .

dct:created a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:created"@en ;
    skos:scopeNote "For expressing when the documentation (e.g. DPIA Necessity Assessment, or DPIA Procedure, or DPIA outcome) was created"@en .

dct:dateAccepted a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:dateAccepted"@en ;
    skos:scopeNote "For expressing when the documentation (e.g. DPIA Necessity Assessment, or DPIA Procedure, or DPIA outcome) was accepted through audit or approval"@en .

dct:dateSubmitted a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:dateSubmitted"@en ;
    skos:scopeNote "For expressing when the documentation (e.g. DPIA Necessity Assessment, or DPIA Procedure, or DPIA outcome) was submitted for audit or approval"@en .

dct:description a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:description"@en ;
    skos:scopeNote "Indicates a description of the DPIA for human comprehension"@en .

dct:hasPart a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:hasPart"@en ;
    skos:scopeNote "For expressing something contains a DPIA document or process contains as a part. For example, as some dpv:DPIA dct:hasPart DPIANecessityAssessment"@en .

dct:identifier a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:identifier"@en ;
    skos:scopeNote "Indicates an identifier associated with the DPIA documentation or process. Identifiers may be reused from existing systems, or created for the purposes of record management"@en .

dct:isPartOf a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:isPartOf"@en ;
    skos:scopeNote "For expressing a DPIA document or process is part of another. For example, as some DPIANecessityAssessment dct:isPartOf some dpv:DPIA"@en .

dct:isVersionOf a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:isVersionOf"@en ;
    skos:scopeNote "For expressing prior versions or iterations of the DPIA document or process"@en .

dct:modified a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:modified"@en ;
    skos:scopeNote "For expressing when the documentation (e.g. DPIA Necessity Assessment, or DPIA Procedure, or DPIA outcome) was last modified"@en .

dct:subject a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:subject"@en ;
    skos:scopeNote "For expressing the subject of the DPIA document or process, where subject refers to the point of focus. For expressing what is affected or included within the DPIA, please see dct:coverage"@en .

dct:temporal a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:temporal"@en ;
    skos:scopeNote "For expressing the temporal coverage of the DPIA document or process"@en .

dct:title a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:title"@en ;
    skos:scopeNote "Indicates a title of the DPIA for human comprehension"@en .

dct:valid a rdf:Property,
        owl:ObjectProperty ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    skos:prefLabel "dct:valid"@en ;
    skos:scopeNote "For expressing the temporal date or range of validity of the DPIA document or process. This refers to the time period for which the DPIA is considered valid, and does not refer to the temporal period associated with processing (see dct:temporal instead). The assumption is that after this period, the DPIA should be re-evaluated or some process should be triggered"@en .

