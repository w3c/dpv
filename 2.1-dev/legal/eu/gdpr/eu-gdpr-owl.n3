eu-gdpr-owl:IntegrityBreach a rdfs:Class,
        owl:Class ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "EDPB Guidelines 9/2022 on personal data breach notification under GDPR"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataBreach ;
    sw:term_status "accepted"@en ;
    skos:definition "A data breach where there is an unauthorised or accidental alteration of personal data"@en ;
    skos:prefLabel "Integrity Breach"@en .

eu-gdpr-owl:IntegrityConfidentialityPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
