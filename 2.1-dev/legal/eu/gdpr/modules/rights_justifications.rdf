  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#JustificationA12IdentityFailure">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Justification"/>
    <skos:prefLabel xml:lang="en">Justification A12 - Identity Failure</skos:prefLabel>
    <skos:definition xml:lang="en">Justification that a request under A14-21 could not be fulfiled due to lack of identity verification</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/justifications#IdentityVerificationFailure"/>
    <skos:scopeNote xml:lang="en">For justifications where identity cannot be verified and requires additional information, eu-gdpr:A12IdentityVerificationRequired should be used</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art.12-6</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#rights-justifications-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#JustificationA14EntityAlreadyInformed">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Justification"/>
    <skos:prefLabel xml:lang="en">Justification A14 - Entity Already Informed</skos:prefLabel>
    <skos:definition xml:lang="en">Justification that A14 obligations for providing information do not apply as the data subject already has the information</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/justifications#EntityAlreadyInformed"/>
    <skos:scopeNote xml:lang="en">This justification requires information on how the data subject was provided the information to satify the assertion, which can be provided as a description of comment</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art.14-5a</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
