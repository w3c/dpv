    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Justification A17 - Public Health</skos:prefLabel>
    <skos:definition xml:lang="en">Justification that the A17 right to erasure or to be forgotten could not be completed due to</skos:definition>
