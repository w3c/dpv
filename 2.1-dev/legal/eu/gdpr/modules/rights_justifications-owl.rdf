  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA17ObjectA21">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <skos:definition xml:lang="en">Justification that the A17 right to erasure or to be forgotten is being exercised through A21 right to object where there are no overriding legitimate grounds for the processing (A21-1) or as an objection to direct marketing (A21-2)</skos:definition>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.17-</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Justification A17 - Object A21</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#Objection"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA14DisproportionateEffort">
    <skos:prefLabel xml:lang="en">Justification A14 - Disproportionate Effort</skos:prefLabel>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">GDPR Art.14-5b</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Justification that A14 obligations for providing information will require a disproportionate effort to fulfill</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <skos:scopeNote xml:lang="en">This justification requires information on why the effort is considered disproportionate, such as the amount of time or resources required, which can be provided as a description of comment</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#DisproportionateEffortRequired"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA12InformationRequired">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Justification that the request under A14-21 could not be fulfilled due to additional information being required</skos:definition>
    <skos:prefLabel xml:lang="en">Justification A12 - Information Required</skos:prefLabel>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">If the purpose of this justification is to ask for identity verification, then eu-gdpr:A12IdentityVerificationRequired should be used. The information required can be expressed using dpv:Process, which allows also expressing the purpose for why it is required and relevant dpv:PersonalData categories, or through a comment or description for the justification</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art.12-6</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#InformationRequired"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA14LegallyExempted">
    <skos:definition xml:lang="en">Justification that A14 obligations for providing information is legally exempted</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">This justification requires information on which legal provision provides the exemption, which can be provided as a description of comment, or ideally through dpv:hasApplicableLaw to refer to the specific law or through dpv:hasLegalBasis to refer to the specific legal basis enabling this justification</skos:scopeNote>
    <dct:source xml:lang="en">GDPR Art.14-5c</dct:source>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <skos:prefLabel xml:lang="en">Justification A14 - Legally Exempted</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#LegallyExempted"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA17ChildData">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Justification A17 - Child Data</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">Justification that the A17 right to erasure or to be forgotten is being exercised as the personal data of a child have been collected for information society services referred to in A8(1)</skos:definition>
    <dct:source xml:lang="en">GDPR Art.17-</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#InformationSocietyServicesOffer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA12IdentityFailure">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Justification A12 - Identity Failure</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Justification that a request under A14-21 could not be fulfiled due to lack of identity verification</skos:definition>
    <skos:scopeNote xml:lang="en">For justifications where identity cannot be verified and requires additional information, eu-gdpr:A12IdentityVerificationRequired should be used</skos:scopeNote>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit, Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <dct:source xml:lang="en">GDPR Art.12-6</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#IdentityVerificationFailure"/>
  </rdf:Description>
