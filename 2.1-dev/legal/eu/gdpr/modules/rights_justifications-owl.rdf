  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA17LegalErasure">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Justification A17 - Legal Erasure</skos:prefLabel>
    <skos:definition xml:lang="en">Justification that the A17 right to erasure or to be forgotten is being exercised as the personal data have to be erased for compliance with a legal obligation in Union or Member State law to which the controller is subject</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">GDPR Art.17-</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#LegalObligation"/>
  </rdf:Description>
