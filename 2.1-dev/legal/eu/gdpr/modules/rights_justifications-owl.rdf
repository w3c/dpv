  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#JustificationA17NoLegalBasis">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Justification"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Beatriz Esteves, Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Justification A17 - No Legal Basis</skos:prefLabel>
    <dct:source xml:lang="en">GDPR Art.17-</dct:source>
    <skos:definition xml:lang="en">Justification that the A17 right to erasure or to be forgotten is being exercised as the corresponding consent has been withdrawn and there is no other legal basis for the processing</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/justifications/owl#LegalityLackingObjection"/>
  </rdf:Description>
