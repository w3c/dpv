  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#InterventionControl">
    <skos:prefLabel xml:lang="en">Intervention Control</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:definition xml:lang="en">Control that intervenes in the operations of the event to change some context with the aim of changing the event or the effects</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
