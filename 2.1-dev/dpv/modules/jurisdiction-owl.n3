    skos:prefLabel "Within Device"@en .

dpv-owl:WithinPhysicalEnvironment a rdfs:Class,
        owl:Class,
        dpv-owl:Location ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2020-10-06"^^xsd:date ;
    rdfs:isDefinedBy dpv-owl: ;
    rdfs:subClassOf dpv-owl:LocalLocation ;
    sw:term_status "accepted"@en ;
