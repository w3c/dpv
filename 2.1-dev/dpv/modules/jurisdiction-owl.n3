dpv-owl:WithinDevice a rdfs:Class,
        owl:Class,
        dpv-owl:Location ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2022-06-15"^^xsd:date ;
    dct:modified "2020-10-05"^^xsd:date ;
