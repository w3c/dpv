  <rdf:Description rdf:about="https://w3id.org/dpv/owl#AgeVerification">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Age Verification</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor rdf:nodeID="N9502991e21554b8d8a638a007ebecaa2"/>
    <dct:contributor rdf:nodeID="N8f189b5cf29b4f0098ed886ba83db8b3"/>
