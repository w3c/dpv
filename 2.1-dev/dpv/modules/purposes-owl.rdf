  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ServiceRegistration">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Beatriz Esteves</dct:contributor>
    <skos:definition xml:lang="en">Purposes associated with registering users and collecting information required for providing a service</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">An example of service registration is to provide a form that collects information such as preferred language or media format for downloading a movie</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Service Registration</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ServiceProvision"/>
  </rdf:Description>
