    <skos:prefLabel xml:lang="en">Often Frequency</skos:prefLabel>
    <skos:definition xml:lang="en">Frequency where occurrences are often or frequent, but not continuous</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Frequency"/>
