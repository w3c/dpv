    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Entity Involvement</skos:prefLabel>
    <skos:definition xml:lang="en">Involvement of an entity in specific context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#ProcessingContext"/>
