  <rdf:Description rdf:about="https://w3id.org/dpv/owl#IndeterminateDuration">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Duration"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
