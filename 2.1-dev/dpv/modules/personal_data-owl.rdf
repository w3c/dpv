  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DerivedPersonalData">
    <skos:prefLabel xml:lang="en">Derived Personal Data</skos:prefLabel>
    <dct:contributor rdf:nodeID="N22e07d100cf44e059efbab5bb10b883b"/>
    <dct:contributor rdf:nodeID="N9502991e21554b8d8a638a007ebecaa2"/>
    <dct:contributor rdf:nodeID="N989459c0e87947339d31658108a104b3"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0009"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0046"/>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/data#Derived"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <dct:source rdf:nodeID="Nf1a3e6a8f2d44bd6a24f37bcd819bd2b"/>
    <skos:scopeNote xml:lang="en">Derived Data is data that is obtained through processing of existing data, e.g. deriving first name from full name. To indicate data that is derived but which was not present or evident within the source data, InferredPersonalData should be used.</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Personal Data that is obtained or derived from other data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DerivedData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PersonalData"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#InferredPersonalData">
