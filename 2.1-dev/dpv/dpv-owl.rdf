  <rdf:Description rdf:about="https://w3id.org/dpv/owl#LegalCompliance">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
