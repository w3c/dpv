    <dct:contributor>Axel Polleres, Javier Fernndez, Harshvardhan J. Pandit, Delaram Golpayegani</dct:contributor>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0004"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0040"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0043"/>
