  <rdf:Description rdf:about="https://w3id.org/dpv#ProvidedPersonalData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Provided Personal Data</skos:prefLabel>
    <skos:definition xml:lang="en">Personal Data that has been provided by an entity such as the Data Subject</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#CollectedPersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#ProvidedData"/>
