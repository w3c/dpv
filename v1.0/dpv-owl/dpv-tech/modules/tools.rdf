    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-tech#DataStorageTechnology"/>
    <rdfs:label xml:lang="en">Identity Wallet</rdfs:label>
    <dct:description xml:lang="en">product and service that allows the user to store identity data, credentials and attributes linked to her/his identity, to provide them to relying parties on request and to use them for authentication, online and offline, and to create qualified electronic signatures and seals</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
