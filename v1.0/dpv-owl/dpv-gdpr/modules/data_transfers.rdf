    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-gdpr#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/dpv-gdpr#AdHocContractualClauses">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-gdpr#DataTransferTool"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Contract"/>
    <rdfs:label xml:lang="en">AdHoc Contractual Clauses</rdfs:label>
    <dct:description xml:lang="en">Contractual Clauses not drafted by the EU Commission, e.g. by the Controller</dct:description>
    <dct:source rdf:resource="https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
