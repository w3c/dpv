    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Purpose ;
    skos:definition "Purposes associated with analysing the usage, delivery, and provisioning metrics of a service"@en ;
    skos:inScheme p7012:purpose-classes ;
    skos:prefLabel "Service Analytics"@en .

