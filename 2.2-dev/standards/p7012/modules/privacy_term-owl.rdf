    <skos:prefLabel xml:lang="en">Portability Term</skos:prefLabel>
    <skos:definition xml:lang="en">Term that indicates portability of data from service provider to individual</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/standards/p7012/owl#"/>
  </rdf:Description>
