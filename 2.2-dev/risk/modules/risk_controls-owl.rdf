    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <skos:scopeNote xml:lang="en">Detection refers to the observation, derivation, inference, or any other method for drawing conclusions that an event has occurred or is likely to occur with a given certainty. For controls that identify information about the event in terms of metrics or characteristics, see risk:IdentificationControl</skos:scopeNote>
    <skos:definition xml:lang="en">Control that detects an event</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:modified>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
