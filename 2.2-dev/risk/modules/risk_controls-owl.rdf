    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <skos:prefLabel xml:lang="en">Change Consequence</skos:prefLabel>
    <skos:definition xml:lang="en">Control that proactively changes the consequence event such that one event is replaced with the occurence or applicability of another event in the context</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:modified>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#SubstitutionControl"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ConsequenceControl"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#OversightControl">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskControl"/>
