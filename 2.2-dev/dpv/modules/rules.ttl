    skos:inScheme dpv:rules-classes ;
    skos:prefLabel "Permission"@en .

dpv:PermissionNotUtilised a rdfs:Class,
        skos:Concept,
        dpv:RuleFulfilmentStatus ;
    dct:contributor _:person-harshvardhan-j-pandit ;
