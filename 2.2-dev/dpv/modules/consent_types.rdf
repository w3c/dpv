    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples#E0018"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#consent-types-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#UninformedConsent">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LegalBasis"/>
    <skos:prefLabel xml:lang="en">Uninformed Consent</skos:prefLabel>
    <skos:definition xml:lang="en">Consent that is uninformed i.e. without requirement to provide sufficient information to make a consenting decision</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Consent"/>
