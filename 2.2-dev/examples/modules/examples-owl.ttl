dex-owl:E0084 a dex-owl:Example ;
    dct:contributor _:N75d91ca4cd5549b6b15c7dffba092807 ;
    dct:created "2025-01-01"^^xsd:date ;
    dct:description "This example shows how the status of rules can be expressed in terms of their fulfilment or violation." ;
