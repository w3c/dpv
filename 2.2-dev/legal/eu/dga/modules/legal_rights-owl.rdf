    <skos:definition xml:lang="en">Right of affected natural and legal persons to an effective judicial remedy</skos:definition>
    <dct:source xml:lang="en">DGA 28</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Right"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
