  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DataSubject">
    <skos:definition xml:lang="en">Data Subject' means a natural person who can be identified, directly or indirectly, in particular by reference to an identifier such as a name, an identification number, location data, an online identifier or to one or more factors specific to the physical, physiological, genetic, mental, economic, cultural or social identity of that natural person</skos:definition>
    <dct:source xml:lang="en">GDPR Art.4-1</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataSubject"/>
    <dct:contributor rdf:nodeID="N75d91ca4cd5549b6b15c7dffba092807"/>
    <skos:prefLabel xml:lang="en">Data Subject</skos:prefLabel>
