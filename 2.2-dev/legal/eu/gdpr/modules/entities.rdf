  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#ThirdParty">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Third Party</skos:prefLabel>
    <skos:definition xml:lang="en">‘Third Party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#ThirdParty"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#ThirdParty"/>
