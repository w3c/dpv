  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#JustificationA33RiskMitigated">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Justification"/>
    <skos:prefLabel xml:lang="en">Justification A33 - Risk Mitigated</skos:prefLabel>
    <skos:definition xml:lang="en">Justification that the personal data breach was not communicated to the data subject as the controller has taken subsequent measures which ensure that the high risk to the rights and freedoms of data subjects is no longer likely to materialise</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#DataBreachJustification"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/justifications#RiskMitigated"/>
