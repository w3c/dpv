  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#GeneticData">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/pd/owl#Genetic"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:value>dpv:PersonalData</rdf:value>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">‘Genetic Data’ means personal data relating to the inherited or acquired genetic characteristics of a natural person which give unique information about the physiology or the health of that natural person and which result, in particular, from an analysis of a biological sample from the natural person in question</skos:definition>
    <skos:prefLabel xml:lang="en">Genetic Data</skos:prefLabel>
    <dct:contributor rdf:nodeID="N75d91ca4cd5549b6b15c7dffba092807"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/pd/owl#Genetic"/>
  </rdf:Description>
