  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AuthorisedRepresentative">
    <skos:prefLabel xml:lang="en">Authorised Representative</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by the AI Act</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Representative"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
    <dct:source xml:lang="en">AIA (Art. 3(5))</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIDeployer">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
    <dct:source xml:lang="en">AIA (Art. 3(4))</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Deployer"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:prefLabel xml:lang="en">AI Deployer</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person, public authority, agency or other body using an AI system under its authority except where the AI system is used in the course of a personal non-professional activity</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIOffice">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <skos:definition xml:lang="en">Commission’s function of contributing to the implementation, monitoring and supervision of AI systems and AI governance. References in the AI Act to the Artificial Intelligence office shall be understood as references to the Commission.</skos:definition>
    <skos:prefLabel xml:lang="en">Artificial Intelligence Office</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
    <dct:source xml:lang="en">AIA(Art. 3(42))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AIOperator">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Operator"/>
