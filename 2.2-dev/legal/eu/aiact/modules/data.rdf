  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#TrainingData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Training Data</skos:prefLabel>
    <skos:definition xml:lang="en">Data used for training an AI system through fitting its learnable parameters</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/ai#TrainingData"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/ai#TrainingData"/>
