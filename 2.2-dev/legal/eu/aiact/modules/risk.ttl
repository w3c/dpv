eu-aiact:Risk a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N8ad4a2e60e4243a68d9cb7715700c92d ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(1a))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf dpv:Risk ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Risk ;
    skos:definition "The combination of the probability of an occurrence of harm and the severity of that harm"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Risk"@en .

eu-aiact:RiskManagementSystem a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N8ad4a2e60e4243a68d9cb7715700c92d ;
    dct:created "2024-06-12"^^xsd:date ;
    dct:source "AI Act"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf risk:RiskManagement ;
    sw:term_status "accepted"@en ;
    skos:broader risk:RiskManagement ;
    skos:definition "A risk management system is a continuous iterative process planned and run throughout the entire lifecycle of a high-risk AI system, requiring regular systematic review and updating"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Risk Management System"@en .

eu-aiact:SeriousIncident a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N8ad4a2e60e4243a68d9cb7715700c92d ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(44))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf risk:Incident ;
    sw:term_status "accepted"@en ;
    skos:broader risk:Incident ;
    skos:definition "Any incident or malfunctioning of an AI system that directly or indirectly leads to any of the following: (a) the death of a person or serious damage to a person’s health;  (b) a serious and irreversible disruption of the management and operation of critical infrastructure.; (ba) breach of obligations under Union law intended to protect fundamental rights; (bb) serious damage to property or the environment."@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Serious Incident"@en .

eu-aiact:SystemicRiskForEU a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N8ad4a2e60e4243a68d9cb7715700c92d ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art.3 (44d))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf eu-aiact:Risk ;
    sw:term_status "accepted"@en ;
    skos:broader eu-aiact:Risk ;
    skos:definition "A systemic risk at Union level where it is specific to the high-impact capabilities of general-purpose AI models, having a significant impact on the internal market due to its reach, and with actual or reasonably foreseeable negative effects on public health, safety, public security, fundamental rights, or the society as a whole, that can be propagated at scale across the value chain"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Systemic Risk for EU"@en .

