  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AIImporter">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">AI Importer</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union that places on the market an AI system that bears the name or trademark of a natural or legal person established outside the Union</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Importer"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact#AIOperator"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Importer"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/aiact#AIOperator"/>
    <dct:source xml:lang="en">AIA (Art. 3(6))</dct:source>
