    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AIProductManufacturer">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">AI Product Manufacturer</skos:prefLabel>
    <skos:definition xml:lang="en">Entity that manufactures an AI Product</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Manufacturer"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#Manufacturer"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
