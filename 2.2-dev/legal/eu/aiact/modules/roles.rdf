    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#AuthorisedRepresentative">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Authorised Representative</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by the AI Act</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Representative"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact#AIOperator"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Representative"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/aiact#AIOperator"/>
    <dct:source xml:lang="en">AIA (Art. 3(5))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/aiact#roles-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact#MarketSurveillanceAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Market Surveillance Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The national authority carrying out the activities and taking the measures pursuant to Regulation (EU) 2019/1020</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact#NationalCompetentAuthority"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/eu/aiact#NationalCompetentAuthority"/>
    <dct:source xml:lang="en">AIA(Art. 3(26))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor rdf:nodeID="N8ad4a2e60e4243a68d9cb7715700c92d"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
