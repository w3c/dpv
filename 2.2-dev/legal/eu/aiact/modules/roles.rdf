    <skos:prefLabel xml:lang="en">AI Distributor</skos:prefLabel>
    <skos:definition xml:lang="en">Any natural or legal person in the supply chain, other than the provider or the importer, that makes an AI system available on the Union market</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech#Distributor"/>
