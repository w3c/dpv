  {
    "@id": "https://w3id.org/dpv/legal/eu/aiact/owl#SeriousIncident",
    "@type": [
      "http://www.w3.org/2000/01/rdf-schema#Class",
      "http://www.w3.org/2002/07/owl#Class"
    ],
    "http://purl.org/dc/terms/contributor": [
