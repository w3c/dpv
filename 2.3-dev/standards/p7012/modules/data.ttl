p7012:NonServiceData a rdfs:Class,
        skos:Concept,
        dpv:PersonalData ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PersonalData ;
    skos:definition "Data other than what is collected, produced, or otherwise generated in the context of a Service"@en ;
    skos:inScheme p7012:data-classes ;
    skos:prefLabel "Non-Service Data"@en .

p7012:ServiceAnalyticsData a rdfs:Class,
        skos:Concept,
        dpv:PersonalData ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PersonalData ;
    skos:definition "Data collected, produced, or otherwise generated in the context of analysing the usage and provision of a Service"@en ;
    skos:inScheme p7012:data-classes ;
    skos:prefLabel "Service Analytics Data"@en .

p7012:ServiceData a rdfs:Class,
        skos:Concept,
        dpv:PersonalData ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PersonalData ;
    skos:definition "Data collected, produced, or otherwise generated in the context of a Service"@en ;
    skos:inScheme p7012:data-classes ;
    skos:prefLabel "Service Data"@en .

p7012:ServiceUsageData a rdfs:Class,
        skos:Concept,
        dpv:PersonalData ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy p7012: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PersonalData ;
    skos:definition "Data collected, produced, or otherwise generated in the context of using a Service"@en ;
    skos:inScheme p7012:data-classes ;
    skos:prefLabel "Service Usage Data"@en .

