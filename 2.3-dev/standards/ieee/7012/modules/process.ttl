ieee-7012:Agreement a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:ContractualClause ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ContractualClause ;
    skos:definition "A compound set of terms or clauses, proposed and offered before a formal contract between parties, and also referred to as a \"proposal\" pr \"proposed agreement\""@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement"@en .

ieee-7012:AgreementInteractionRecord a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:RecordsOfActivities ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:RecordsOfActivities ;
    skos:definition "A record of a interactions conducted in the course of establishing and negotiating agreements, including failed attempts, creation of contracts, and subsequent interactions"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement Interaction Record"@en .

ieee-7012:AgreementNegotiation a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:Process ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Process ;
    skos:definition "A negotiation between parties regarding an agreement"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement Negotiation"@en .

ieee-7012:AgreementNegotiationRequest a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:Process ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Process ;
    skos:definition "A request to initiate negotiation with the goal of finding an acceptable agreement"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement Negotiation Request"@en .

ieee-7012:AgreementPreference a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:Rule ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Rule ;
    skos:definition "A preference regarding agreements, such as to indicate which terms are acceptable or unacceptable"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement Preference"@en .

ieee-7012:AgreementRegistry a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    sw:term_status "accepted"@en ;
    skos:definition "A registry hosting agreements that acts as a common repository for parties to utilise as a reference within requests and for negotiating acceptable terms"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Agreement Term Registry"@en .

ieee-7012:Endpoint a rdfs:Class,
        skos:Concept ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:modified "2026-02-03"^^xsd:date ;
    rdfs:isDefinedBy ieee-7012: ;
    rdfs:subClassOf dpv:Location ;
    sw:term_status "modified"@en ;
    skos:broader dpv:Location ;
    skos:definition "A location that acts as an endpoint, such as a URL, for carrying out communications and interactions between parties as defined in IEEE 7012"@en ;
    skos:inScheme ieee-7012:process-classes ;
    skos:prefLabel "Endpoint"@en .

