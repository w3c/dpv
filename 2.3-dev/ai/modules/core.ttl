ai:AI a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-28"^^xsd:date ;
    dct:source "ISO/IEC 22989:2022"@en ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf dpv:Technology ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Technology ;
    skos:definition "A technical and scientific field devoted to the engineered system that generates outputs such as content, forecasts, recommendations or decisions for a given set of human-defined objectives"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "Artificial Intelligence (AI)"@en .

ai:AIAgent a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-28"^^xsd:date ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI,
        tech:SoftwareAgent ;
    sw:term_status "accepted"@en ;
    skos:broader ai:AI,
        tech:SoftwareAgent ;
    skos:definition "An AI Agent, also known as an 'intelligent agent', is a software agent that utilises AI technologies"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "AI Agent"@en ;
    skos:scopeNote "Other definitions of AI Agents may include the perception of environment as some capacity to incorporate the environment through sensors (e.g. computer vision) or data (e.g. web environments), and to produce changes in outputs through some degree of autonomy (i.e. without human intervention). The definition of 'AI Agent' provided in this extension relaxes such requirements and instead focuses on classifying these agents based solely on their use of AI technologies to allow compatibility with the evolving definitions"@en .

ai:AIProcess a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-harshvardhan-j-pandit ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf dpv:Process ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Process ;
    skos:definition "A process involving AI technologies"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "AI Process"@en ;
    skos:scopeNote "This concept provides a convenient method to indicate AI is involved (somewhere) within the Process"@en .

ai:AISystem a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-05-17"^^xsd:date ;
    dct:source " ISO/IEC 22989:2022"@en,
        "OECD.AI"@en ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI,
        tech:System ;
    sw:term_status "accepted"@en ;
    skos:broader ai:AI,
        tech:System ;
    skos:definition "An engineered system that generates outputs such as content, forecasts, recommendations or decisions for a given set of human-defined objectives (ISO/IEC 22989:2022 definition); or A machine-based system that, for explicit or implicit objectives, infers, from the input it receives, how to generate outputs such as predictions, content, recommendations, or decisions that can influence physical or virtual environments. Different AI systems vary in their levels of autonomy and adaptiveness after deployment (OECD 2024 definition); or A machine-based system demonstrating varying degrees of autonomy and adaptiveness after deployment, generating outputs such as predictions, content, recommendations, or decisions to influence physical or virtual environments (EU Vocabularies' AI Taxonomy definition)"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "AI System"@en .

ai:Capability a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-28"^^xsd:date ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI ;
    sw:term_status "accepted"@en ;
    skos:broader ai:AI ;
    skos:definition "Capability or use of AI to achieve a technical goal or objective"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "Capability"@en ;
    skos:scopeNote "This concept refers to the application of an AI technique to achieve a technical goal or function, and is necessary to distinguish the 'algorithm' (ai:Technique) from the 'application' (ai:Capability) and 'goal' (dpv:Purpose)"@en .

ai:GenAI a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-12-17"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "AI Taxonomy - EU Vocabularies" ;
            schema:url "http://data.europa.eu/8s8/ai-taxonomy" ] ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI ;
    sw:term_status "accepted"@en ;
    skos:broader ai:AI ;
    skos:definition "Use of artificial intelligence models that can learn from and mimic large amounts of data to create content such as text, images, music, videos, code, and more, based on inputs or prompts"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "Generative AI"@en .

ai:Model a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-05-17"^^xsd:date ;
    dct:modified "2025-12-17"^^xsd:date ;
    dct:source " ISO/IEC 22989:2022"@en,
        "OECD.AI"@en ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI,
        tech:Model ;
    sw:term_status "modified"@en ;
    skos:broader ai:AI,
        tech:Model ;
    skos:definition "Physical, mathematical or otherwise logical representation of a system, entity, phenomenon, process or data involving the use of AI techniques"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "Model"@en .

ai:Technique a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-28"^^xsd:date ;
    rdfs:isDefinedBy ai: ;
    rdfs:subClassOf ai:AI ;
    sw:term_status "accepted"@en ;
    skos:broader ai:AI ;
    skos:definition "The underlying technological algorithm, method, or process that forms the technique for using or applying AI"@en ;
    skos:inScheme ai:core-classes ;
    skos:prefLabel "Technique"@en ;
    skos:scopeNote "This concept refers to the foundational computational implementation and is necessary to distinguish the 'algorithm' (ai:Technique) from the 'application' (ai:Capability) and 'goal' (dpv:Purpose)"@en .

