  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Location">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Location</rdfs:label>
    <dct:description xml:lang="en">A location is a position, site, or area where something is located</dct:description>
    <rdfs:comment xml:lang="en">Location may be geographic, physical, or virtual.</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
