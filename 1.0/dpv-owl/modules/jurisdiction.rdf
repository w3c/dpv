    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#VariableLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LocationFixture"/>
    <rdfs:label xml:lang="en">Variable Location</rdfs:label>
    <dct:description xml:lang="en">Location that is known but is variable e.g. somewhere within a given area</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#PrivateLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LocalLocation"/>
