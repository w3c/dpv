    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Region">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Location"/>
    <rdfs:label xml:lang="en">Region</rdfs:label>
    <dct:description xml:lang="en">A region is an area or site that is considered a location</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
