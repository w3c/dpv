    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#WithinVirtualEnvironment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LocalLocation"/>
    <rdfs:label xml:lang="en">Within Virtual Environment</rdfs:label>
    <dct:description xml:lang="en">Location is local and entirely within a virtual environment, such as a shared network directory</dct:description>
