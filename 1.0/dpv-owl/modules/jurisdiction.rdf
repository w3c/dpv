    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#RandomLocation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl#LocationFixture"/>
    <rdfs:label xml:lang="en">Random Location</rdfs:label>
    <dct:description xml:lang="en">Location that is random or unknown</dct:description>
