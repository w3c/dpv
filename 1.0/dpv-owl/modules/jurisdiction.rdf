  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#SupraNationalUnion">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Location"/>
    <rdfs:label xml:lang="en">Supranational Union</rdfs:label>
    <dct:description xml:lang="en">A political union of two or more countries with an establishment of common authority</dct:description>
