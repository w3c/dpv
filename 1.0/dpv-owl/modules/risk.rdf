    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#ConsequenceOfFailure">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Consequence"/>
    <rdfs:label xml:lang="en">Consequence of Failure</rdfs:label>
    <dct:description xml:lang="en">The consequence(s) possible or arising from failure of specified context</dct:description>
