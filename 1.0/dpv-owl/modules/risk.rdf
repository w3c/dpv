    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Detriment">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Impact"/>
    <rdfs:label xml:lang="en">Detriment</rdfs:label>
    <dct:description xml:lang="en">Impact that acts as or causes detriments</dct:description>
