  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#hasResidualRisk">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="https://w3id.org/dpv/dpv-owl#Risk"/>
    <rdfs:range rdf:resource="https://w3id.org/dpv/dpv-owl#Risk"/>
    <rdfs:label xml:lang="en">has residual risk</rdfs:label>
    <dct:description xml:lang="en">Indicates the associated risk is the remaining or residual risk from applying mitigation measures or treatments to this risk</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-20</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
