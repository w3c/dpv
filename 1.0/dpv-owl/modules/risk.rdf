    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Severity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Severity</rdfs:label>
    <dct:description xml:lang="en">The magnitude of being unwanted or having negative effects such as harmful impacts</dct:description>
    <rdfs:comment xml:lang="en">Severity can be associated with Risk, or its Consequences and Impacts</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-21</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
