    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#Purpose"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#ServiceOptimisation"/>
    <skos:prefLabel xml:lang="en">Optimisation for Consumer</skos:prefLabel>
    <skos:definition xml:lang="en">Purposes associated with optimisation of activities and services for consumer or user</skos:definition>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/purposes#Custom"/>
    <skos:note xml:lang="en">The term optmisation here refers to the efficiency of the service in terms of technical provision (or similar means) with benefits for everybody. Personalisation implies making changes that benefit the current user or persona.</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
