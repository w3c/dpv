  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#Impact">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#Consequence"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#Consequence"/>
    <skos:prefLabel xml:lang="en">Impact</skos:prefLabel>
    <skos:definition xml:lang="en">The impact(s) possible or arising as a consequence from specified context</skos:definition>
    <skos:note xml:lang="en">Impact is a stronger notion of consequence in terms of influence, change, or effect on something e.g. for impact assessments</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
