  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#isResidualRiskOf">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:domain rdf:resource="https://w3id.org/dpv/dpv-skos#Risk"/>
    <rdfs:range rdf:resource="https://w3id.org/dpv/dpv-skos#Risk"/>
    <skos:prefLabel xml:lang="en">is residual risk of</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates this risk is the remaining or residual risk from applying mitigation measures or treatments to specified risk</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-20</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Julian Flake</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#Risk">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Risk</skos:prefLabel>
    <skos:definition xml:lang="en">A risk or possibility or uncertainty of negative effects, impacts, or consequences.</skos:definition>
    <skos:note xml:lang="en">Risks can be associated with one or more different concepts such as purpose, processing, personal data, technical or organisational measure.</skos:note>
