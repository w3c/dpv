    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#DataSubject">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <skos:prefLabel xml:lang="en">Data Subject</skos:prefLabel>
    <skos:definition xml:lang="en">The individual (or category of individuals) whose personal data is being processed</skos:definition>
    <skos:note xml:lang="en">The term 'data subject' is specific to the GDPR, but is functionally equivalent to the term 'individual' and the ISO/IEC term 'PII Principle'.</skos:note>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_1/oj"/>
