  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ProfessionalConfidentialData">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Professional Confidential Data</skos:prefLabel>
    <skos:definition xml:lang="en">Data protected by professional secrecy or confidentiality, including but not limited to data covered by professional privilege or secrecy obligations such as that covered by lawyer or doctor-patient confidentiality and other forms of recognised professional confidentiality obligations</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
