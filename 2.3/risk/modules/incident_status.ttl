risk:IncidentSuspected a rdfs:Class,
        skos:Concept,
        risk:IncidentStatus ;
    dct:contributor _:person-harshvardhan-j-pandit ;
    dct:created "2024-02-14"^^xsd:date ;
    rdfs:isDefinedBy risk: ;
    sw:term_status "accepted"@en ;
    skos:broader risk:IncidentStatus ;
