        rdfs:isDefinedBy <https://w3id.org/dpv/ai/owl#>,
        skos:definition "Capability for generation of artificial speech"@en,
        skos:prefLabel "Speech Synthesis"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/ai/owl#AudioCapability>,
        <https://w3id.org/dpv/ai/owl#ContentGeneration>
    
    
Class: <https://w3id.org/dpv/ai/owl#TextClassification>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/ai/owl#>,
        skos:definition "Capability of assigning predefined labels to text data in order to automatically categorise it into groups"@en,
        skos:prefLabel "Text Classification"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/ai/owl#NaturalLanguageProcessing>
    
    
Class: <https://w3id.org/dpv/ai/owl#TextDataMining>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/ai/owl#>,
        skos:definition "Capability of selecting and analysing large amounts of text or data resources to discover patterns, relationships, and semantic insights that provide valuable information for research and decision-making"@en,
        skos:prefLabel "Text and Data Mining"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/ai/owl#NaturalLanguageProcessing>
    
    
Class: <https://w3id.org/dpv/ai/owl#VideoGeneration>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/ai/owl#>,
        skos:definition "Capability to generate video"@en,
        skos:prefLabel "Video Generation"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/ai/owl#ContentGeneration>
    
    
Class: <https://w3id.org/dpv/ai/owl#VisualRecognition>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/ai/owl#>,
        skos:definition "Capability that identifies and categorises objects, scenes, activities, and other visual elements in images or video, and includes image classification, object detection, scene understanding, and visual pattern recognition"@en,
        skos:prefLabel "Visual Recognition"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/ai/owl#ComputerVision>
    
    
