  <rdf:Description rdf:about="https://w3id.org/dpv/sector/health/owl#InteroperabilitySoftwareForEHRSystem">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/sector/health/owl#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-28</dct:created>
    <skos:definition xml:lang="en">Software component of the EHR system which provides and receives personal electronic health data under a priority category for primary use established under the EHDS Regulation in the European electronic health record exchange format</skos:definition>
    <skos:prefLabel xml:lang="en">Interoperability Software for EHR System</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Software"/>
  </rdf:Description>
