    <skos:prefLabel xml:lang="en">Electronic Health Data Access Service</skos:prefLabel>
    <skos:definition xml:lang="en">Technical service, such as an online portal or application, that allows data subjects to access their electronic health data or the electronic health data of those natural persons whose electronic health data they are legally authorised to access</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Service"/>
  </rdf:Description>
