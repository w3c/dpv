  <rdf:Description rdf:about="https://w3id.org/dpv/sector/health/owl#ElectronicHealthDataAccessService">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Technology"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
