de-gdng:HealthDataAccessApplication a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-draeger,
        _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2025-10-13"^^xsd:date ;
    dct:source "4"@en,
        "5"@en,
        "[[GDNG]] § 3(2) point 3"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Process ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Process ;
    skos:definition "Prozess zur Beantragung von Zugang zu Gesundheitsdaten bei datenhaltenden Stellen durch Datennutzende, der durch die DACO unterstützt und koordiniert wird"@de,
        "Process of application by data users for access to health data stored at health data holders. The DACO supports data users with this process and coordinates communication"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Antrag auf Zugang zu Gesundheitsdaten"@de,
        "Health Data Access Application"@en .

de-gdng:HealthDataAccessApplicationRecord a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-draeger,
        _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2026-02-09"^^xsd:date ;
    dct:source "4"@en,
        "5"@en,
        "7"@en,
        "[[GDNG]] § 3(2) point 3"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Data ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Data ;
    skos:definition "Im Rahmen der Antragsstellung des Zugangs auf Gesundheitsdaten zu erfassende und anfallende Daten, die auch im öffentlichen Antragsregister veröffentlicht werden"@de,
        "In course of health data access applications collected and produced data that is also published in the public health data access application register"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Gesundheitsdatenzugangsantragsdaten"@de,
        "Health Data Access Application Record"@en .

de-gdng:HealthDataQuery a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-draeger,
        _:person-christian-henke,
        _:person-julian-flake,
        _:person-matthias-lobe ;
    dct:created "2025-10-13"^^xsd:date ;
    dct:source "[[GDNG]] § 3(2) point 2"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Process ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Process ;
    skos:definition "Prozess der Identifizierung und Lokalisierung von Gesundheitsdaten durch Datennutzende für ihre Zwecke, welche dazu von der DACO beraten werden"@de,
        "Process of identification and localization of health data by health data users for their purposes, supported by the DACO"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Identifizierung und Lokalisierung von Gesundheitsdaten"@de,
        "Indentification and Localization of Health Data"@en .

de-gdng:HealthDataQueryRecord a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-draeger,
        _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2026-02-09"^^xsd:date ;
    dct:source "[[GDNG]] § 3(2) point 2"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Data ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Data ;
    skos:definition "Die im Rahmen der Identifikation und Lokalisierung von Gesundheitsdaten zu erfassenden und anfallende Daten"@de,
        "Data that is collected or produced during the identification and localization of health data"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Gesundheitsdatenanfragedaten"@de,
        "Health Data Query Record"@en .

de-gdng:PublicHealthDataAccessApplicationRegister a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2025-10-13"^^xsd:date ;
    dct:source "[[GDNG]] § 3(2) point7"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Service ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Service ;
    skos:definition "Von der DACO gepflegtes, öffentliches Register mit Informationen zu den über die DACO gestellten Anträgen auf Zugang zu Gesundheitsdaten, zu den Datennutzenden, zu den Vorhaben, für die Daten beantragt wurden, und zu deren Ergebnissen"@de,
        "A public registry of information about health data access applications, the corresponding data users, research projects and requested data, maintained by the DACO"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Öffentliches Antragsregister"@de,
        "Public Health Data Access Application Register"@en .

de-gdng:PublicMetadataCatalogue a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2025-10-13"^^xsd:date ;
    dct:source "[[GDNG]] § 3(2) point 1"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:Service ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Service ;
    skos:definition "Von der DACO barrierefrei zu führender und pflegender Katalog, in dem zu Transparenzzwecken Informationen über die im deutschen Gesundheitswesen vorhandenen und öffentlich zugänglichen Gesundheitsdaten und über die jeweiligen Halter dieser Daten gesammelt werden"@de,
        "An accessible catalog, driven and maintained by the DACO, in which information about existing and publicly available health data of the german health care along with the respective data holder are collected to fulfill transparency obligations"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Öffentlicher Metadaten-Katalog"@de,
        "Public Metadata Catalogue"@en .

de-gdng:SecureProcessingEnvironment a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-christian-henke,
        _:person-julian-flake ;
    dct:created "2025-10-13"^^xsd:date ;
    dct:source "[[GDNG]] § 3(2) point 9"@en ;
    rdfs:isDefinedBy de-gdng: ;
    rdfs:subClassOf dpv:SecureProcessingEnvironment ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:SecureProcessingEnvironment ;
    skos:definition "Durch die DACO konzeptionierte Maßnahme zur Verbesserung des Datenschutzes und der Datensicherheit im Rahmen der Weiterverarbeitung von Gesundheitsdaten zu Forschungszwecken und weiteren im Gemeinwohl liegenden Zwecken"@de,
        "A measure for improvement of data protection and information security in context of processing of health data for research purposes and common welfare, drafted by the DACO"@en ;
    skos:inScheme de-gdng:query-and-access-classes ;
    skos:prefLabel "Sichere Verarbeitungsumgebung"@de,
        "Secure Processing Environment"@en .

