  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#Anonymization">
    <skos:prefLabel xml:lang="de">Anonymisierung von Gesundheitsdaten</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Anonymization of Data</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
