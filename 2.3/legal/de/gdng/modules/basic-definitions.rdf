  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#SecondaryUse">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Sekundärdatennutzung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Secondary Use</skos:prefLabel>
    <skos:definition xml:lang="de">Weiterverarbeitung von Gesundheitsdaten zu anderen Zwecken als denjenigen, für die die Daten ursprünglich erhoben wurden</skos:definition>
    <skos:definition xml:lang="en">Processing of health data for purposes other than those for which the data was originally collected</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 2 point 8</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#basic-definitions-classes"/>
  </rdf:Description>
