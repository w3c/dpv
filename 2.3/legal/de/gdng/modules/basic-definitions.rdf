  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#ResearchProject">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Forschungsvorhaben</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Research Project</skos:prefLabel>
    <skos:definition xml:lang="de">Ein Forschungsvorhaben im Sinne von Artikel 9 Absatz 2 Buchstabe j und Artikel 89 Absatz 1 Satz 1 und Absatz 2 der DSGVO [[GDPR]]</skos:definition>
    <skos:definition xml:lang="en">A research project in the sense of Art. 9(2)(j) and Art. 89(1), first sentence, and (2) [[GDPR]]</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 2 point 5</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
