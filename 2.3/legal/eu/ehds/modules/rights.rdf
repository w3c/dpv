    <rdf:type rdf:resource="https://w3id.org/dpv#Right"/>
    <skos:prefLabel xml:lang="en">A6 Right of natural persons to rectification</skos:prefLabel>
    <skos:definition xml:lang="en">Right of natural persons to request online the rectification of their personal electronic health data</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Right"/>
    <dct:source xml:lang="en">EHDS Art.6</dct:source>
