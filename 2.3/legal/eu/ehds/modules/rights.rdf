  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/ehds#A9">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Right"/>
    <skos:prefLabel xml:lang="en">A9 Right to obtain information on accessing data</skos:prefLabel>
    <skos:definition xml:lang="en">Right of natural persons to obtain information, including through automatic notifications, on any access to their personal electronic health data through the health professional access service obtained in the context of healthcare</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Right"/>
    <dct:source xml:lang="en">EHDS Art.9</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-28</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
