Datatype: rdf:langString

    
Datatype: xsd:string

    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#AuthorisedRepresentative>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Legal representative established in the EU of a manufacturer of an EHR system established outside of the EU"@en,
        skos:prefLabel "Authorised Representative"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EHDSBoard>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "An authority tasked with facilitating the cooperation and the exchange of information among Member States and the Commission for primary and secondary use"@en,
        skos:prefLabel "European Health Data Space Board"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#HealthDataAccessBody>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "An entity governed by public law responsible for granting access to electronic health data for secondary use"@en,
        skos:prefLabel "Health Data Access Body"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#HealthDataHolder>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "An entity in the healthcare sector who has the right to process personal electronic health data or make available non-personal electronic health data"@en,
        skos:prefLabel "Health Data Holder"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#HealthDataUser>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "An entity in the healthcare sector who was granted access to electronic health data for secondary use"@en,
        skos:prefLabel "Health Data User"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#TrustedHealthDataHolder>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "A health data holder that meets the conditions for the simplified procedure to access electronic health data"@en,
        skos:prefLabel "Trusted Health Data Holder"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/legal/eu/ehds/owl#HealthDataHolder>
    
    
