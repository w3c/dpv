Class: <https://w3id.org/dpv/legal/eu/ehds/owl#DigitalHealthAuthorityActivityReport>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Document containing an overview of activities reported by a digital health authority"@en,
        skos:prefLabel "Digital Health Authority Activity Report"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EHRSystemInformationSheet>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Technical documentation with information related to the EHR system, including its manufacturer, intended purpose and supported standards"@en,
        skos:prefLabel "EHR System Information Sheet"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EHRSystemTechnicalDocumentation>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Technical documentation that demonstrates that the EHR system manufacturers comply with the requirements laid down in Annex II"@en,
        skos:prefLabel "EHR System Technical Documentation"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EUDatabaseForRegistrationOfEHRSystems>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Publicly available database established by the European Commission to maintain data on EHR systems for which an EU declaration of conformity has been issued according to EHDS Article 39"@en,
        skos:prefLabel "EU Database for Registration of EHR Systems"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EUDatabaseForRegistrationOfWellnessApplications>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Publicly available database established by the European Commission to maintain data on wellness applications for which a label has been issued pursuant to EHDS Article 47"@en,
        skos:prefLabel "EU Database for Registration of Wellness Applications"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EUDeclarationOfConformity>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Declaration of conformity by the EU stating that the manufacturer of an EHR system has demonstrated that the essential requirements laid down in EHDS Annex II have been fulfilled"@en,
        skos:prefLabel "EU Declaration of Conformity"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EuropeanDigitalTestingEnvironment>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Data processing environment for the assessment of harmonised software components of EHR systems"@en,
        skos:prefLabel "European Digital Testing Environment"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#WellnessApplicationLabel>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Label issued by the manufacturer of the wellness application that indicating its compliance with the common specifications referred to in EHDS Article 36 and the requirements laid down in EHDS Annex II"@en,
        skos:prefLabel "Wellness Application Label"@en
    
    
