eu-gdpr-owl:BalancingTest a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:ProportionalityAssessment,
        dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "An assessment of whether the individual's interests override the interests of the controller"@en ;
    skos:prefLabel "Balancing Test"@en ;
    skos:scopeNote "While Balancing Test can be conducted on its own, it is an integral part of the Proportionality Assessment process"@en .

eu-gdpr-owl:Disproportionate a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:ProportionalityStatus ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:ProportionalityStatus ;
    sw:term_status "accepted"@en ;
    skos:definition "Status representing the activity or context is disproportionate i.e. it is not proportionate"@en ;
    skos:prefLabel "Disproportionate"@en ;
    skos:scopeNote "A Disproportinate status indicates that the process should not be continued as it risks impacting the rights of the data subject in an unacceptable manner"@en .

eu-gdpr-owl:NecessityAssessment a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:ProportionalityAssessment,
        dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "An assessment of whether the processing of personal data is necessary to achieve the stated aims, where necessary indicates a lack of other means which could have been used instead"@en ;
    skos:prefLabel "Necessity Assessment"@en ;
    skos:scopeNote "While Necessity can be assessed on its own, it is an integral part of the Proportionality Assessment process"@en .

eu-gdpr-owl:ProportionalityAssessment a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:ImpactAssessment ;
    sw:term_status "accepted"@en ;
    skos:definition "Proportionality is a general principle of EU law, which when applied for the GDPR requires striking a balance between the processing of personal data and the resulting impacts on rights. More simply, the processing should not result in a disproportionate impact on rights"@en ;
    skos:prefLabel "Proportionality Assessment"@en ;
    skos:scopeNote "The assessment of proportionality requires an assessment of suitability (represented by `eu-gdpr:SuitabilityAssessment`), necessity (represented by `eu-gdpr:NecessityAssessment`), and a balancing test (represented by `eu-gdpr:BalancingTest`). The outcome of the proportionality assessment is represented as an instance of `eu-gdpr:ProportionalityStatus`"@en .

eu-gdpr-owl:ProportionalityStatus a rdfs:Class,
        owl:Class,
        dpv-owl:Status ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Status ;
    sw:term_status "accepted"@en ;
    skos:definition "Status indicating whether the specific activity or context is proportionate"@en ;
    skos:prefLabel "Proportionality Status"@en .

eu-gdpr-owl:Proportionate a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:ProportionalityStatus ;
    dct:contributor _:person-efstratios-koulierakis,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2025-10-10"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:ProportionalityStatus ;
    sw:term_status "accepted"@en ;
    skos:definition "Status representing the activity or context is proportionate"@en ;
    skos:prefLabel "Proportionate"@en .

eu-gdpr-owl:SuitabilityAssessment a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor _:person-efstratios-koulierakis ;
    dct:created "2026-02-06"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:ProportionalityAssessment,
        dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "An assessment of whether the processing of personal data is suitable to achieve the stated aims"@en ;
    skos:prefLabel "Suitability Assessment"@en ;
    skos:scopeNote "While Suitability Assessment can be carried out on its own, it is an integral part of the Proportionality Assessment process"@en .

