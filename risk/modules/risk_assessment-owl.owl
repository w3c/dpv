  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#MonteCarloSimulation">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:definition xml:lang="en">Calculates the probability of outcomes by running multiple simulations using random variables.</skos:definition>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Monte Carlo Simulation</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#CausalMapping">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Causal Mapping</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:definition xml:lang="en">A network diagram representing events, causes and effects and their relationships.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Fishbone">
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Identifies contributory factors to a defined outcome (wanted or unwanted). Contributory factors are usually divided into predefined categories and displayed in a tree structure or a fishbone diagram.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:prefLabel xml:lang="en">Ishikawa (Fishbone)</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#GameTheory">
    <skos:definition xml:lang="en">The study of strategic decision making to model the impact of the decisions of different players involved in the game. Example application area can be risk based pricing.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Game Theory</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#CauseConsequenceAnalysis">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Cause-Consequence Analysis</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">A combination of fault and event tree analysis that allows inclusion of time delays. Both causes and consequences of an initiating event are considered.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Interviews">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Structured or semi- structured one-to-one conversations to elicit views.</skos:definition>
    <skos:prefLabel xml:lang="en">Interviews</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#BayesianAnalysis">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A means of making inference about model parameters using Bayes' theorem which has the capability of incorporating empirical data into prior judgements about probabilities</skos:definition>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:prefLabel xml:lang="en">Bayesian Analysis</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ALARA">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">As Low as Resonably Achievable (ALARP) - A criteria for deciding significance of risk and means of evaluating tolerability of risk</skos:definition>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">ALARA</skos:prefLabel>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#PIA">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">Analyses how incidents and events could affect a person's privacy and identifies and quantifies the capabilities that would be needed to manage it.</skos:definition>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:prefLabel xml:lang="en">Privacy Impact Analysis (PIA)</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Toxicological">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:definition xml:lang="en">A series of steps taken to obtain a measure for the risk to humans or ecological systems due to exposure to chemicals.</skos:definition>
    <skos:prefLabel xml:lang="en">Toxicological Risk Assessment</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#RiskMatrix">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <skos:prefLabel xml:lang="en">Risk Matrix</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Compares individual risks by selecting a consequence/ likelihood pair and displaying them on a matrix with consequence on one axis and likelihood on the other.</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#FaultTreeAnalysis">
    <skos:definition xml:lang="en">Analyses causes of a focus event using Boolean logic to describe combinations of faults. Variations include a success tree where the top event is desired and a cause tree used to investigate past events.</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Fault Tree Analysis</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#ALARP">
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:prefLabel xml:lang="en">ALARP</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <skos:definition xml:lang="en">As Low as Resonably Possible (ALARP) - A criteria for deciding significance of risk and means of evaluating tolerability of risk</skos:definition>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#CrossImpactAnalysis">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <skos:definition xml:lang="en">Evaluates changes in the probability of the occurrence of a given set of events consequent on the actual occurrence of one of them.</skos:definition>
    <skos:prefLabel xml:lang="en">Cross Impact Analysis</skos:prefLabel>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Taxonomies">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:prefLabel xml:lang="en">Taxonomies</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:definition xml:lang="en">A taxonomy based on experience or on concepts and models that can be used to help identify risks or controls.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Surveys">
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:definition xml:lang="en">Paper- or computer-based questionnaires to elicit views.</skos:definition>
    <skos:prefLabel xml:lang="en">Surveys</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#BusinessImpactAnalysis">
    <skos:definition xml:lang="en">A process that analyses the consequences of a disruptive incident on the organization which determines the recovery priorities of an organization's products and services and, thereby, the priorities of the activities and resources which deliver them</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:prefLabel xml:lang="en">Business Impact Analysis</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QuantitativeRiskAnalysis"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk/owl#Brainstorming">
    <rdf:type rdf:resource="https://w3id.org/dpv/risk/owl#RiskAnalysis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk/owl#"/>
    <dct:source xml:lang="en">(IEC 31010:2019,https://www.iso.org/standard/72140.html)</dct:source>
    <skos:prefLabel xml:lang="en">Brainstorming</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <skos:definition xml:lang="en">Technique used in workshops to encourage imaginative thinking</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#QualitativeRiskAnalysis"/>
  </rdf:Description>
