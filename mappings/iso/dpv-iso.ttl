@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix dpv: <https://w3id.org/dpv#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix dpv-iso: <https://w3id.org/dpv/mappings/iso#> .

dpv-iso:PII a skos:Concept ;
    skos:prefLabel "Personally Identifiable Information (PII)" ;
    dct:source "ISO/IEC 29100:2024" ;
    skos:narrower dpv:PersonalData .

dpv-iso:PIIController a skos:Concept ;
    skos:prefLabel "PII Controller" ;
    dct:source "ISO/IEC 29100:2024" ;
    skos:exactMatch dpv:DataController .

dpv-iso:PIIPrincipal a skos:Concept ;
    skos:prefLabel "PII Principal" ;
    dct:source "ISO/IEC 29100:2024" ;
    skos:exactMatch dpv:DataSubject .

dpv-iso:PIIProcessor a skos:Concept ;
    skos:prefLabel "PII Processor" ;
    dct:source "ISO/IEC 29100:2024" ;
    skos:exactMatch dpv:DataProcessor .

dpv-iso:Consent a skos:Concept ;
    skos:prefLabel "Consent" ;
    dct:source "ISO/IEC 29100:2024" ;
    skos:exactMatch dpv:InformedConsent .
