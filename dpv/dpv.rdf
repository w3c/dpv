  <rdf:Description rdf:about="https://w3id.org/dpv#ExplicitlyExpressedConsent">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
