  <rdf:Description rdf:about="https://w3id.org/dpv#SensitivePersonalData">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
