  <rdf:Description rdf:about="https://w3id.org/dpv#OptimiseUserInterface">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Optimise User Interface</skos:prefLabel>
    <skos:definition xml:lang="en">Purposes associated with optimisation of interfaces presented to the user</skos:definition>
