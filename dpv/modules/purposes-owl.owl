    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#VendorManagement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#FulfilmentOfContractualObligation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Fulfilment of Contractual Obligation</skos:prefLabel>
    <skos:definition xml:lang="en">Purposes associated with carrying out data processing to fulfill a contractual obligation</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P Krog, Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
