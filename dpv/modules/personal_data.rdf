    <skos:narrower rdf:resource="https://w3id.org/dpv#GeneratedPersonalData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#SensitivePersonalData"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
