    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DerivedPersonalData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#DerivedPersonalData"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#personal-data-classes">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
