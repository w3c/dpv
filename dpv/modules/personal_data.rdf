    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DerivedPersonalData"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#ObservedPersonalData"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#GeneratedPersonalData"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#SensitivePersonalData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#PseudonymisedData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#IdentifyingPersonalData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#CollectedPersonalData"/>
