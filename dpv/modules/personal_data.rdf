    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#SensitiveNonPersonalData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">SensitiveNonPersonalData</skos:prefLabel>
    <skos:definition xml:lang="en">Non-personal data deemed sensitive</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#SensitiveData"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#SensitiveData"/>
    <dct:source xml:lang="en">DGA 30(a)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#InferredData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Inferred Data</skos:prefLabel>
    <skos:definition xml:lang="en">Data that has been obtained through inferences of other data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Data"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Data"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#GeneratedPersonalData"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#GeneratedPersonalData"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#DerivedData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Derived Data</skos:prefLabel>
    <skos:definition xml:lang="en">Data that has been obtained through derivations of other data</skos:definition>
