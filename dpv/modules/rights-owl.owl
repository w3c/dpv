    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasRight">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has right</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates use or applicability of Right</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv#Right"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv#Right"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-18</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
