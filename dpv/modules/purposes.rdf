  <rdf:Description rdf:about="https://w3id.org/dpv#Personalisation">
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#Purpose"/>
    <skos:prefLabel xml:lang="en">Personalisation</skos:prefLabel>
    <skos:definition xml:lang="en">Create and provide customisation based on attributes and/or needs of person(s) or context(s).</skos:definition>
    <skos:note xml:lang="en">This term is a blanket purpose category for indicating personalisation of some other purpose, e.g. by creating a subclass of the other concept and Personalisation</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-01</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
  </rdf:Description>
