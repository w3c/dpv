    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">has impact</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates impact(s) possible or arising as consequences from specified concept</skos:definition>
