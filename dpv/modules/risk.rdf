    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#risk-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasConsequenceOn">
