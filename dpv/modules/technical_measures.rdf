    <skos:prefLabel xml:lang="en">Deterministic Pseudonymisation</skos:prefLabel>
    <skos:definition xml:lang="en">Pseudonymisation achieved through a deterministic function</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Pseudonymisation"/>
