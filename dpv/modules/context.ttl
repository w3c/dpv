        dpv:hasDuration,
        dpv:hasFrequency,
        dpv:hasHumanInvolvement,
        dpv:hasIdentifier,
        dpv:isAfter,
        dpv:isBefore,
        dpv:isImplementedByEntity,
        dpv:isImplementedUsingTechnology .

dpv:isInstanceOf skos1:prefLabel "isInstanceOf"@en ;
