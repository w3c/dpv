dpv:ContextConcepts a skos:Collection ;
    dct:title "Context Concepts"^^xsd:string ;
    skos:member dpv:AlgorithmicLogic,
        dpv:Context,
        dpv:Duration,
        dpv:Frequency,
        dpv:HumanInvolvement,
        dpv:Importance,
        dpv:Necessity,
        dpv:NotRequired,
        dpv:Optional,
        dpv:PrimaryImportance,
        dpv:Required,
        dpv:SecondaryImportance,
        dpv:Technology,
        dpv:hasAlgorithmicLogic,
