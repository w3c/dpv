    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Procedure where the background of an entity is assessed to identity vulnerabilities and threats due to their current or intended role</skos:definition>
    <skos:prefLabel xml:lang="en">Background Checks</skos:prefLabel>
