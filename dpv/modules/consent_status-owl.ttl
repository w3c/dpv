    dct:modified "2024-01-01"@en ;
    dct:title "Data Privacy Vocabulary (DPV)"@en ;
    vann:preferredNamespacePrefix "dpv" ;
    vann:preferredNamespaceUri "https://w3id.org/dpv#" ;
    schema:version "2" .

dpv:Status rdfs:superClassOf dpv:ConsentStatus .

