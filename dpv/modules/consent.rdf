  <rdf:Description rdf:about="https://w3id.org/dpv#hasExpiry">
    <rdf:type rdf:resource="https://w3id.org/dpv#Relation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">has expiry</skos:prefLabel>
    <skos:definition xml:lang="en">Generic property specifying when or under which condition(s) the consent will expire</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
