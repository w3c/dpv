  <rdf:Description rdf:about="https://w3id.org/dpv#hasWithdrawalBy">
    <rdf:type rdf:resource="https://w3id.org/dpv#Relation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:range rdf:resource="https://w3id.org/dpv#LegalEntity"/>
    <skos:prefLabel xml:lang="en">has withdrawal by</skos:prefLabel>
    <skos:definition xml:lang="en">Specifies the entity that withdrew consent</skos:definition>
    <skos:note xml:lang="en">Normally this would be the dataSubject, but in some exceptional cases, the consent might be withdraawn on behalf by someone else, e.g. parents of minors.</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
