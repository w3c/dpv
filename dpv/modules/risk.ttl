dpv:Impact a skos:Concept,
        dpv:Concept ;
    dct:created "2022-03-23"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Fajar Ekaputra"^^xsd:string,
        "Georg P Krog"^^xsd:string,
