    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A safeguard is a precautionary measure for the protection against or mitigation of negative effects</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#HumanInvolvementForDecision">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#HumanInvolvement"/>
