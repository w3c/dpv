  <rdf:Description rdf:about="https://w3id.org/dpv/owl#VitalInterest">
    <skos:prefLabel xml:lang="en">Vital Interest</skos:prefLabel>
    <skos:definition xml:lang="en">Processing is necessary or required to protect vital interests of a data subject or other natural person</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
