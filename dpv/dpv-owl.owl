  <rdf:Description rdf:about="https://w3id.org/dpv/owl#EnvironmentalProtection">
    <dct:source xml:lang="en">NIST SP 800-13</dct:source>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <skos:prefLabel xml:lang="en">Environmental Protection</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#PhysicalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-14</dct:created>
    <skos:definition xml:lang="en">Physical protection against environmental threats such as fire, floods, storms, etc.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
