    <schema:domainIncludes rdf:resource="https://w3id.org/dpv/owl#RightExerciseActivity"/>
    <skos:definition xml:lang="en">Indicates the specified concepts is 'after' this concept in some context</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
