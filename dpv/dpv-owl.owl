  <rdf:Description rdf:about="https://w3id.org/dpv/owl#isImplementedByEntity">
    <skos:scopeNote xml:lang="en">The use of 'entity' is inclusive of entities (e.g. Data Processor) as well as 'agent' (e.g. DPO). For indicating technological implementation, the property isImplementedByTechnology should be used.</skos:scopeNote>
    <skos:scopeNote xml:lang="en">Indicates the Entity that implements or performs a Right Exercise Activity</skos:scopeNote>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/owl#Entity"/>
