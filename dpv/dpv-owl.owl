  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DocumentSecurity">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#TechnicalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:source xml:lang="en">(ENISA Reference Incident Classification Taxonomy 2018,https://www.enisa.europa.eu/publications/reference-incident-classification-taxonomy/)</dct:source>
