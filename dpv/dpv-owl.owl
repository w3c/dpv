    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0018"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0025"/>
