    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#MonotonicCounterPseudonymisation"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#RNGPseudonymisation"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#FullyRandomisedPseudonymisation"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DeterministicPseudonymisation"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DocumentRandomisedPseudonymisation"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Deidentification"/>
  </rdf:Description>
