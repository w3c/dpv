    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Intended</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani, Georg P. Krog, Paul Ryan</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#IntentionStatus"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Consumer">
