    <rdfs:label xml:lang="en">Art 9(2-i)</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalBasis"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">public interest in public health</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_i/oj"/>
  </rdf:Description>
