  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#A49-1-g">
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_g/oj"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalBasis"/>
    <rdfs:comment xml:lang="en">Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist.</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:label xml:lang="en">Art 49(1-g)</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:description xml:lang="en">The transfer is made from a register which according to Union or Member State law is intended to provide information to the public in general or by any person who can demonstrate a legitimate interest, but only to the extent that the conditions laid down by Union or Member State law for consultation are fulfilled in the particular case.</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
