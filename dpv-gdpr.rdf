  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#A46-2-f">
    <dct:description xml:lang="en">An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to appy the appropriate safeguards, including as regards individuals` rights</dct:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#LegalBasis"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_f/oj"/>
