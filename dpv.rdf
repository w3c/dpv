  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#PseudoAnonymization">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_5/pnt_g/oj"/>
