    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#DataSubjectRight">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Right"/>
    <rdfs:label xml:lang="en">Data Subject Right</rdfs:label>
    <dct:description xml:lang="en">The rights applicable or provided to a Data Subject</dct:description>
    <rdfs:comment xml:lang="en">Based on use of definitions, the notion of 'Data Subject Right' can be equivalent to 'Individual Right' or 'Right of a Person'</rdfs:comment>
