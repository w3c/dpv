  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#DataSubject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LegalEntity"/>
    <rdfs:label xml:lang="en">Data Subject</rdfs:label>
    <dct:description xml:lang="en">The individual (or category of individuals) whose personal data is being processed</dct:description>
    <rdfs:comment xml:lang="en">The term 'data subject' is specific to the GDPR, but is functionally equivalent to the term 'individual' and the ISO/IEC term 'PII Principle'.</rdfs:comment>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_1/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
