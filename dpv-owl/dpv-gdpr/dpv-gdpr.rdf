    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#OfficialAuthorityOfController"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#PublicInterest"/>
