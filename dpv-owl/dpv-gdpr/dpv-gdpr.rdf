    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdfs:comment xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</rdfs:comment>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-gdpr#"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_c/oj"/>
    <rdfs:label xml:lang="en">Art 49(1-c) conclusion of contract</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/dpv-gdpr#A46-2-e">
