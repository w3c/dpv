    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_a/oj"/>
    <dct:description xml:lang="en">explicit consent with special categories of data</dct:description>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
