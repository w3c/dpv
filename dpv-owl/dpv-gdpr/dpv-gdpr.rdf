    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-gdpr#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#DataSubjectRight"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_20/oj"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">A20 Right to Data Portability</rdfs:label>
    <dct:description xml:lang="en">Right to data portability</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/dpv-gdpr#A45-3">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#DataTransferLegalBasis"/>
    <rdfs:comment xml:lang="en">Transfer from EU to a third country. Third country has Adequacy Decision.</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">Art 45(3) adequacy decision</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
