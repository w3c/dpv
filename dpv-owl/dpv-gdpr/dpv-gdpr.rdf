    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rigo Wenning</dct:creator>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Consent"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl/dpv-gdpr#"/>
    <rdfs:comment xml:lang="en">Valid consent in this case would have requirements for being 'explicit' in addition to requirements defined by A4-11. This is also mentioned in the Article 29 Working Party document "Guidelines on Consent under Regulation 2016/679 (wp259rev.01)"</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_a/oj"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">Art 6(1-a) explicit consent</rdfs:label>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-10</dct:created>
    <dct:description xml:lang="en">explicit' consent of the data subject</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/dpv-gdpr#A49-1-b">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
