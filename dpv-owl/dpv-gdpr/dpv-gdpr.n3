dpvo-gdpr:A45-3 a owl:Class ;
    rdfs:label "Art 45(3)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Personal data can flow freely from the EU to a third country with an Adequacy Decision without any further safeguard being necessary."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_45/par_3/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has Adequacy Decision."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-a a owl:Class ;
    rdfs:label "Art 46(2-a)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "A legally binding and enforceable instrument between public authorities or bodies"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_a/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-b a owl:Class ;
    rdfs:label "Art 46(2-b)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Binding corporate rules"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_b/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo-gdpr:BindingCorporateRules ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-c a owl:Class ;
    rdfs:label "Art 46(2-c)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Standard data protection clauses adopted by the Commission"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo-gdpr:SCCByCommission ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-d a owl:Class ;
    rdfs:label "Art 46(2-d)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Standard data protection clauses adopted by a Supervisory Authority"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_d/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority"@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo-gdpr:SCCBySupervisoryAuthority ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-e a owl:Class ;
    rdfs:label "Art 46(2-e)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "An approved code of conduct pursuant to GDPR Article 40 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards individuals´ rights"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_e/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-2-f a owl:Class ;
    rdfs:label "Art 46(2-f)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to appy the appropriate safeguards, including as regards individuals` rights"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_f/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-3-a a owl:Class ;
    rdfs:label "Art 46(3-a)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Contractual clauses with controller, processor or recipient of the personal data in the third country or the international organisation."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_3/pnt_a/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards exist. Transfer does requires specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A46-3-b a owl:Class ;
    rdfs:label "Art 46(3-b)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "Provisions to be inserted into administrative arrangements between public authorities or bodies which include enforceable and effective data subject rights"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_3/pnt_b/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards exist. Transfer does requires specific authorisation from a Supervisor Authority."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-a a owl:Class ;
    rdfs:label "Art 49(1-a)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:Consent,
        dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-b a owl:Class ;
    rdfs:label "Art 49(1-b)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is necessary for the performance of a contract between the data subject and controller or the implementation of pre-contractual measures taken at the data subject´s request."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_b/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:Contract,
        dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-c a owl:Class ;
    rdfs:label "Art 49(1-c)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is necessary for the conclusion or performance of a contract concluded in the interest of the data subject and controller and another natural or legal person."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_c/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:Contract,
        dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-d a owl:Class ;
    rdfs:label "Art 49(1-d)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is necessary for important reasons of public interest."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_d/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo:PublicInterest ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-e a owl:Class ;
    rdfs:label "Art 49(1-e)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is necessary for the establishment, exercise or defence of legal claims."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_e/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-f a owl:Class ;
    rdfs:label "Art 49(1-f)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the person is physically or legally incapable of giving consent."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_f/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo:VitalInterestOfNatualPerson ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-1-g a owl:Class ;
    rdfs:label "Art 49(1-g)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is made from a register which according to Union or Member State law is intended to provide information to the public in general or by any person who can demonstrate a legitimate interest, but only to the extent that the conditions laid down by Union or Member State law for consultation are fulfilled in the particular case."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_g/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A49-2 a owl:Class ;
    rdfs:label "Art 49(2)"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P Krog"^^xsd:string ;
    dct:description "The transfer is not repetetive, concerns only a limited number of data subjects, is necessary for the purposes of compelling legitimate interests pursued by controller which are not overridden by the interests or rights and freedoms of the data subject, and controller has assessed all the circumstances surrounding the data transfer and have on the basis of that assessment provided suitable safeguards with regard to the protection of personal data."@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_2/oj> ;
    rdfs:comment "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist and no other options apply."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:DataTransferLegalBasis,
        dpvo:LegitimateInterest ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A6-1-a-explicit-consent a owl:Class ;
    rdfs:label "Art 6(1-a) explicit consent"@en ;
    dct:created "2019-04-10"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Eva Schlehahn"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Rigo Wenning"^^xsd:string ;
    dct:description "explicit' consent of the data subject"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_a/oj> ;
    rdfs:comment "Valid consent in this case would have requirements for being 'explicit' in addition to requirements defined by A4-11. This is also mentioned in the Article 29 Working Party document \"Guidelines on Consent under Regulation 2016/679 (wp259rev.01)\""@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
    rdfs:subClassOf dpvo:Consent ;
    sw:term_status "accepted"@en .

dpvo-gdpr:A6-1-a-non-explicit-consent a owl:Class ;
    rdfs:label "Art.6(1-a) regular consent"@en ;
    dct:created "2019-04-10"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Eva Schlehahn"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Rigo Wenning"^^xsd:string ;
    dct:description "consent of the data subject"@en ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_a/oj> ;
    rdfs:comment "Definition of consent: A data subject's unambigious/clear affirmative action that signifies an agreement to process their personal data (Rigo Wenning) . What is referred to as 'non-explicit consent' here is also termed as 'regular' consent in the Article 29 Working Party document \"Guidelines on Consent under Regulation 2016/679 (wp259rev.01)\". This is the legal basis that requires consent but not at the level of being 'explicit'."@en ;
    rdfs:isDefinedBy dpvo-gdpr: ;
