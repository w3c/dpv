  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#ExplicitlyExpressedConsent">
    <dct:description xml:lang="en">Consent that is expressed through an explicit action solely conveying a consenting decision</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#ExpressedConsent"/>
