    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-01</dct:created>
    <dct:description xml:lang="en">Communication Management refers to purposes associated with providing or managing communication activities e.g. to send an email for notifying some information</dct:description>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
