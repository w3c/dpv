    ],
    "http://purl.org/dc/terms/description": [
      {
        "@language": "en",
        "@value": "A 'child' is a natural legal person who is below a certain legal age depending on the legal jurisdiction."
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#comment": [
      {
        "@value": "A 'child' is distinct from a 'minor'. For example, the legal age for a 'minor' in most countries is 18, whereas a 'child' can be a minor below the age of 14."
      }
    ],
