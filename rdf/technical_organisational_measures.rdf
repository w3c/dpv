    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://www.iso.org/iso-31000-risk-management.html"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#OrganisationalMeasure"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:comment xml:lang="en">Data Protection Impact Assessments as per GDPR art 35, other Privacy Impact Assessments, threat severity assessment https://www.cnil.fr/en/privacy-impact-assessment-pia</rdfs:comment>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rob Brennan</dct:creator>
  </rdf:Description>
