    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#InterruptionControl"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#ImpactControl"/>
  </rdf:Description>
