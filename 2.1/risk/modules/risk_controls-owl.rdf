    <skos:definition xml:lang="en">Risk Mitigation Measure that controls Impacts</skos:definition>
    <dct:contributor rdf:nodeID="N0a43dd9ea5e94fe695d8b7df8802cfdd"/>
    <skos:prefLabel xml:lang="en">Impact Control</skos:prefLabel>
