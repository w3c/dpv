  <rdf:Description rdf:about="https://w3id.org/dpv/risk#AvoidConsequence">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskControl"/>
    <skos:prefLabel xml:lang="en">Avoid Consequence</skos:prefLabel>
    <skos:definition xml:lang="en">Control that proactively avoids the consequence such that it has a reduced exposure or applicability in the context</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#ConsequenceControl"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#AvoidanceControl"/>
