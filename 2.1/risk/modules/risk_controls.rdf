  <rdf:Description rdf:about="https://w3id.org/dpv/risk#ReversalControl">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskControl"/>
    <skos:prefLabel xml:lang="en">Reversal Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that aims to reverse or undo the effects of an event</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#ResolutionControl"/>
    <skos:scopeNote xml:lang="en">Reversal can be achieved through measures such as restoring information from a backup or replacing things such that the initial context not containing the event's effects is restored</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="N9c63c24625fe482386a350b4ea27b4f8"/>
    <dct:contributor rdf:nodeID="Ne22a67b17c7e4a779c30d2864b6a0cee"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/risk#risk-controls-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/risk#ModificationControl">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskControl"/>
    <skos:prefLabel xml:lang="en">Modification Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that modifies the context to change the event's characteristics such that the event still occurs but with the modified characteristics with the goal of managing an event that is accepted to occur</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#ProactiveControl"/>
    <skos:scopeNote xml:lang="en">Modification necessitates a change in the context to result in a change in the event's characteristics. Where such changes reduce the likelihood or effects of the event, the modification also works as a mitigation for the event</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor rdf:nodeID="N9c63c24625fe482386a350b4ea27b4f8"/>
    <dct:contributor rdf:nodeID="Ne22a67b17c7e4a779c30d2864b6a0cee"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/risk#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/risk#risk-controls-classes"/>
  </rdf:Description>
