  <rdf:Description rdf:about="https://w3id.org/dpv/risk#MitigationControl">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/risk#RiskControl"/>
    <skos:prefLabel xml:lang="en">Mitigation Control</skos:prefLabel>
    <skos:definition xml:lang="en">Control that aims to reduce the likelihood or effect of an event with the goal of managing an event accepted to occur</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/risk#ProactiveControl"/>
    <skos:scopeNote xml:lang="en">Mitigation requires accepting that an event will occur, and thereby focusing on managing it by reducing its likelihood or effects by adding additional processes to specifically address the event or its effects</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-06</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
