  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#IdentifyingPersonalData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Identifying Personal Data</skos:prefLabel>
    <skos:definition xml:lang="en">Personal Data that explicitly and by itself is sufficient to identify a person</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <skos:scopeNote xml:lang="en">DPV does not use PII ('Personally Identifiable Information') as it has varying and conflicting definitions across sources. Instead the concept 'identifying personal data' is intended to provide a clear categorisation of its interpretation. Where multiple data categories can be combined to create an 'identifying' category e.g. fingerprinting, this concept represents the combined category.</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-02-14</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#DerivedData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Derived Data</skos:prefLabel>
    <skos:definition xml:lang="en">Data that has been obtained through derivations of other data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Data"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Data"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#personal-data-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#DerivedPersonalData">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Derived Personal Data</skos:prefLabel>
    <skos:definition xml:lang="en">Personal Data that is obtained or derived from other data</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#DerivedData"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#DerivedData"/>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/data#Derived"/>
    <skos:scopeNote xml:lang="en">Derived Data is data that is obtained through processing of existing data, e.g. deriving first name from full name. To indicate data that is derived but which was not present or evident within the source data, InferredPersonalData should be used.</skos:scopeNote>
    <dct:source rdf:nodeID="N8c7a5d06b0274134b701b6aff27c0453"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
