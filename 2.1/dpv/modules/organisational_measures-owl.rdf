    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-12-17</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
  </rdf:Description>
