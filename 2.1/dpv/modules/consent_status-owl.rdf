  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentStatus">
    <dct:contributor rdf:nodeID="N05cd1f43dbaf4b4ba297dab5ee221394"/>
    <dct:contributor rdf:nodeID="N0c29df1c09e34406bfb949a8e7afb527"/>
