  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentRefused">
    <dct:source rdf:nodeID="N1005a46b97534591a6df96e4b66ea326"/>
    <skos:prefLabel xml:lang="en">Consent Refused</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor rdf:nodeID="N05cd1f43dbaf4b4ba297dab5ee221394"/>
    <dct:contributor rdf:nodeID="Nd0960ac69a57403fb5db2455be10d160"/>
    <dct:contributor rdf:nodeID="N0a43dd9ea5e94fe695d8b7df8802cfdd"/>
    <dct:contributor rdf:nodeID="N0c29df1c09e34406bfb949a8e7afb527"/>
    <skos:definition xml:lang="en">The state where consent has been refused</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <skos:scopeNote xml:lang="en">An example of this state is when the individual clicks on a 'disagree' or 'reject' or 'refuse' button, or leaves a checkbox unticked</skos:scopeNote>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentUnknown">
    <dct:source rdf:nodeID="N940b98a9e97b4e6ab05d868091d66072"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">State where information about consent is not available or is unknown</skos:definition>
    <skos:scopeNote xml:lang="en">Consent states can be unknown, for example, when information is not available, or cannot be trusted, or is known to be inaccurate</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:contributor rdf:nodeID="N0c29df1c09e34406bfb949a8e7afb527"/>
    <dct:contributor rdf:nodeID="N05cd1f43dbaf4b4ba297dab5ee221394"/>
    <dct:contributor rdf:nodeID="Nd0960ac69a57403fb5db2455be10d160"/>
    <dct:contributor rdf:nodeID="N0a43dd9ea5e94fe695d8b7df8802cfdd"/>
    <skos:prefLabel xml:lang="en">Consent Unknown</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing">
    <dct:contributor rdf:nodeID="N05cd1f43dbaf4b4ba297dab5ee221394"/>
    <dct:contributor rdf:nodeID="N0a43dd9ea5e94fe695d8b7df8802cfdd"/>
    <dct:contributor rdf:nodeID="N0c29df1c09e34406bfb949a8e7afb527"/>
    <dct:contributor rdf:nodeID="Nd0960ac69a57403fb5db2455be10d160"/>
    <skos:scopeNote xml:lang="en">This identifies the stages associated with consent that should not be used to process data</skos:scopeNote>
    <skos:definition xml:lang="en">States of consent that cannot be used as valid justifications for processing data</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source rdf:nodeID="N344581e2622a4bdaa58b0a62c95da49e"/>
    <skos:prefLabel xml:lang="en">Consent Status Invalid for Processing</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
  </rdf:Description>
