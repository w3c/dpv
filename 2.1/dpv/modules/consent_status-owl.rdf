    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Practically, given consent is the only valid state for processing</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <skos:definition xml:lang="en">States of consent that can be used as valid justifications for processing data</skos:definition>
    <dct:source rdf:nodeID="N536e9ea4d7164b0a9b46907e8bb74262"/>
    <skos:prefLabel xml:lang="en">Consent Status Valid for Processing</skos:prefLabel>
