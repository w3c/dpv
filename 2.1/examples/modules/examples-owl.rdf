    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-11</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples/owl#E0034">
    <dct:description>This example first shows a process where the processor is denoted as the data recipient, and then another process where a third party is denoted as the recipient. A joint controller agreement is then shown which specifies that only one of the controllers involved is a data recipient.</dct:description>
    <dct:contributor rdf:nodeID="N2fe5586a82794e5a99ff2b47c1f2175e"/>
    <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">E0034.ttl</dct:source>
