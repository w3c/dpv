  <rdf:Description rdf:about="https://w3id.org/dpv/examples/owl#E0030">
    <rdf:type rdf:resource="https://w3id.org/dpv/examples/owl#Example"/>
    <dct:subject rdf:resource="https://w3id.org/dpv/owl#Rule"/>
