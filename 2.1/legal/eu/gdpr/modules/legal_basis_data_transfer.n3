    dct:modified "2024-12-17"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "GDPR Art.49-1a" ;
            schema:url "https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj" ] ;
    rdfs:isDefinedBy eu-gdpr: ;
    sw:term_status "modified"@en ;
    skos:broader dpv:DataTransferLegalBasis,
        dpv:ExplicitlyExpressedConsent,
        eu-gdpr:Consent ;
    skos:definition "The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards."@en ;
    skos:inScheme eu-gdpr:legal-basis-data-transfer-classes ;
    skos:prefLabel "Art 49(1-a) explicit consent"@en ;
    skos:scopeNote "Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist."@en .

eu-gdpr:A49-1-b a rdfs:Class,
