    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#A46-2-e">
