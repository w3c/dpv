eu-gdpr:ConcernedSupervisoryAuthority a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf eu-gdpr:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:broader eu-gdpr:DataProtectionAuthority ;
    skos:definition "Concerned Supervisory Authority' or 'supervisory authority concerned’ means a supervisory authority which is concerned by the processing of personal data because: (a) the controller or processor is established on the territory of the Member State of that supervisory authority; (b) data subjects residing in the Member State of that supervisory authority are substantially affected or likely to be substantially affected by the processing; or (c) a complaint has been lodged with that supervisory authority"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Concerned Supervisory Authority"@en .

eu-gdpr:Controller a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-7"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:DataController ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataController ;
    skos:definition "‘Controller’ means the natural or legal person, public authority, agency or other body which, alone or jointly with others, determines the purposes and means of the processing of personal data; where the purposes and means of such processing are determined by Union or Member State law, the controller or the specific criteria for its nomination may be provided for by Union or Member State law"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Controller"@en .

eu-gdpr:DataProtectionAuthority a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-06-22"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataProtectionAuthority ;
    skos:definition "‘Supervisory Authority’ or 'Data Protection Authority' means an independent public authority which is established by a Member State pursuant to Article 51"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Data Protection Authority"@en .

eu-gdpr:DataSubject a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-1"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:DataSubject ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataSubject ;
    skos:definition "Data Subject' means a natural person who can be identified, directly or indirectly, in particular by reference to an identifier such as a name, an identification number, location data, an online identifier or to one or more factors specific to the physical, physiological, genetic, mental, economic, cultural or social identity of that natural person"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Data Subject"@en .

eu-gdpr:Establishment a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " GDPR Art.56"@en,
        " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Recital 22"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Organisation ;
    skos:definition "Establishment is a Legal Entity which implies the effective and real exercise of activities through stable arrangements (with a presumed parent or primary establishment)"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Establishment"@en .

eu-gdpr:InternationalOrganisation a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-26"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:InternationalOrganisation ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:InternationalOrganisation ;
    skos:definition "‘International Organisation’ means an organisation and its subordinate bodies governed by public international law, or any other body which is set up by, or on the basis of, an agreement between two or more countries"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "International Organisation"@en .

eu-gdpr:LeadSupervisoryAuthority a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf eu-gdpr:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:broader eu-gdpr:DataProtectionAuthority ;
    skos:definition "Authority with the primary responsibility for dealing with a cross-border data processing activity"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Lead Supervisory Authority"@en .

eu-gdpr:LocalSupervisoryAuthority a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.56"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf eu-gdpr:DataProtectionAuthority ;
    sw:term_status "accepted"@en ;
    skos:broader eu-gdpr:DataProtectionAuthority ;
    skos:definition "Authority associated with the main or local establishment of an organisation"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Local Supervisory Authority"@en .

eu-gdpr:MainEstablishment a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " GDPR Art.56"@en,
        " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.4-16"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf eu-gdpr:Establishment ;
    sw:term_status "accepted"@en ;
    skos:broader eu-gdpr:Establishment ;
    skos:definition "Main Establishment' means as regards a controller with establishments in more than one Member State, the place of its central administration in the Union, unless the decisions on the purposes and means of the processing of personal data are taken in another establishment of the controller in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment; as regards a processor with establishments in more than one Member State, the place of its central administration in the Union, or, if the processor has no central administration in the Union, the establishment of the processor in the Union where the main processing activities in the context of the activities of an establishment of the processor take place to the extent that the processor is subject to specific obligations under this Regulation;"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Main Establishment"@en .

eu-gdpr:Processor a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-8"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:DataProcessor ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:DataProcessor ;
    skos:definition "‘Processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Processor"@en .

eu-gdpr:Recipient a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-9"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:Recipient ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Recipient ;
    skos:definition "‘Recipient’ means a natural or legal person, public authority, agency or another body, to which the personal data are disclosed, whether a third party or not"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Recipient"@en .

eu-gdpr:Representative a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-17"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:Representative ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Representative ;
    skos:definition "Representative’ means a natural or legal person established in the Union who, designated by the controller or processor in writing pursuant to Article 27, represents the controller or processor with regard to their respective obligations under this Regulation"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Representative"@en .

eu-gdpr:SingleEstablishment a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N05cd1f43dbaf4b4ba297dab5ee221394,
        _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source "GDPR Art.23"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf eu-gdpr:Establishment ;
    sw:term_status "accepted"@en ;
    skos:broader eu-gdpr:Establishment ;
    skos:definition "A legal entity that is established in only one Member State"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Single Establishment"@en .

eu-gdpr:ThirdParty a rdfs:Class,
        skos:Concept ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "GDPR Art.4-10"@en ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:ThirdParty ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:ThirdParty ;
    skos:definition "‘Third Party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data"@en ;
    skos:inScheme eu-gdpr:entities-classes ;
    skos:prefLabel "Third Party"@en .

