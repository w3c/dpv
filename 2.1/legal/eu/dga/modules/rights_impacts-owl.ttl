eu-dga-owl:A28-Impact a rdfs:Class,
        owl:Class,
        dpv-owl:RiskConcept,
        risk-owl:PotentialConsequence,
        risk-owl:PotentialImpact,
        risk-owl:PotentialRisk ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "DGA 28"@en ;
    rdfs:isDefinedBy eu-dga-owl: ;
    rdfs:subClassOf eu-dga-owl:DGARightsImpact ;
    sw:term_status "accepted"@en ;
    skos:definition "Something that acts as or is considered as an impact on Right of affected natural and legal persons to an effective judicial remedy"@en ;
    skos:prefLabel "Impact on A28 Right to an effective judicial remedy"@en .

eu-dga-owl:A9-2-Impact a rdfs:Class,
        owl:Class,
        dpv-owl:RiskConcept,
        risk-owl:PotentialConsequence,
        risk-owl:PotentialImpact,
        risk-owl:PotentialRisk ;
    dct:contributor _:N0a43dd9ea5e94fe695d8b7df8802cfdd ;
    dct:created "2024-12-17"^^xsd:date ;
    dct:source "DGA 9-3"@en ;
    rdfs:isDefinedBy eu-dga-owl: ;
    rdfs:subClassOf eu-dga-owl:DGARightsImpact ;
    sw:term_status "accepted"@en ;
    skos:definition "Something that acts as or is considered as an impact on Right of redress for a natural or legal person directly affected by a decision regarding reuse (A9-1), in the Member State where the relevant body is located"@en ;
    skos:prefLabel "Impact on A9-2 Right to Redress"@en .

eu-dga-owl:DGARightsImpact a rdfs:Class,
