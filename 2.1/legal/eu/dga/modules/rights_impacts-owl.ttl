        dpv-owl:RiskConcept,
        risk-owl:PotentialConsequence,
        risk-owl:PotentialImpact,
        risk-owl:PotentialRisk ;
    dct:created "2024-12-01"^^xsd:date ;
    rdfs:isDefinedBy eu-dga-owl: ;
    rdfs:subClassOf risk-owl:RightsImpact ;
    sw:term_status "accepted"@en ;
    skos:definition "Something that acts as or is considered as an impact on one or more rights defined by DGA"@en ;
    skos:prefLabel "DGA Rights Impact"@en .

