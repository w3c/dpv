  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricIdentityVerification">
    <skos:prefLabel xml:lang="en">Biometric Identity Verification</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
