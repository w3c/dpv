eu-aiact:AILiteracy a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:Ne8d6151e3d8047e6a947b3ec1a3830a1 ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bh))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:AILiteracy ;
    skos:definition "Skills, knowledge and understanding that allows providers, users and affected persons, taking into account their respective rights and obligations in the context of the AI Act, to make an informed deployment of AI systems, as well as to gain awareness about the opportunities and risks of AI and possible harm it can cause."@en ;
    skos:inScheme eu-aiact:misc-classes ;
    skos:prefLabel "AI Literacy"@en .

eu-aiact:IntendedPurpose a rdfs:Class,
        skos:Concept ;
    dct:contributor _:Ne8d6151e3d8047e6a947b3ec1a3830a1 ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(12))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf dpv:Purpose ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Purpose ;
    skos:definition "The use for which an AI system is intended by the provider, including the specific context and conditions of use, as specified in the information supplied by the provider in the instructions for use, promotional or sales materials and statements, as well as in the technical documentation"@en ;
    skos:inScheme eu-aiact:misc-classes ;
    skos:prefLabel "Intended Purpose"@en .

eu-aiact:PubliclyAccessibleSpace a rdfs:Class,
        skos:Concept,
        dpv:Location ;
    dct:contributor _:Ne8d6151e3d8047e6a947b3ec1a3830a1 ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(39))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PublicLocation ;
    skos:definition "Any publicly or privately owned physical place accessible to an undetermined number of natural persons, regardless of whether certain conditions for access may apply, and regardless of the potential capacity restrictions."@en ;
    skos:inScheme eu-aiact:misc-classes ;
    skos:prefLabel "Publicly Accessible Space"@en .

eu-aiact:ReasonablyForeseeableMisuse a rdfs:Class,
        skos:Concept,
        dpv:RiskConcept ;
    dct:contributor _:Ne8d6151e3d8047e6a947b3ec1a3830a1 ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(13))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader risk:Misuse ;
    skos:definition "The use of an AI system in a way that is not in accordance with its intended purpose, but which may result from reasonably foreseeable human behaviour or interaction with other systems, including other AI systems"@en ;
    skos:inScheme eu-aiact:misc-classes ;
    skos:prefLabel "Reasonably Foreseeable Misuse"@en ;
    skos:scopeNote "Misuse should be provided through the Risk extension, along with taxonomy e.g. Misuse -> Foreseeable -> Reasonably and NonReasonably Foreseeable; and Misuse -> Unforeseeable Misuse."@en .

eu-aiact:SubstantialModification a rdfs:Class,
        skos:Concept ;
    dct:contributor _:Ne8d6151e3d8047e6a947b3ec1a3830a1 ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(23))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:definition "A change to the AI system after its placing on the market or putting into service which is not foreseen or planned in the initial conformity assessment by the provider and as a result of which the compliance of the AI system with the requirements set out in Chapter III, Section 2 of the AI Act is affected or results in a modification to the intended purpose for which the AI system has been assessed"@en ;
    skos:inScheme eu-aiact:misc-classes ;
    skos:prefLabel "Substantial Modification"@en .

