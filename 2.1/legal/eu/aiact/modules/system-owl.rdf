  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#SafetyComponent">
    <skos:prefLabel xml:lang="en">Safety Component</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA (Art. 3(14))</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:contributor rdf:nodeID="Ne8d6151e3d8047e6a947b3ec1a3830a1"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/tech/owl#Component"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A component of a product or of a system which fulfils a safety function for that product or system, or the failure or malfunctioning of which endangers the health and safety of persons or property.</skos:definition>
