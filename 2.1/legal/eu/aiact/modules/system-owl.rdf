  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#EmotionRecognitionSystem">
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <skos:related rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#EmotionRecognition"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">An AI system for the purpose of identifying or inferring emotions or intentions of natural persons on the basis of their biometric data.</skos:definition>
    <rdf:type rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
