    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Notice provided regarding fulfilment of a right</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#RightExerciseNotice"/>
  </rdf:Description>
