  <rdf:Description rdf:about="https://w3id.org/dpv/owl#RightFulfilmentNotice">
    <dct:contributor rdf:nodeID="person-beatriz-esteves"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <skos:prefLabel xml:lang="en">Right Fulfilment Notice</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
