  <rdf:Description rdf:about="https://w3id.org/dpv/owl#LegalAgent">
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A Legal Agent is a Legal Entity that is authorised to act on behalf of another entity</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-28</dct:created>
    <skos:scopeNote xml:lang="en">Legal agents may be human or non-human (e.g. an organisation), but they must be 'legal persons' i.e. recognised by law as such. The agency and the mechanisms through which it is given, acquired, or expressed is governed by relevant jurisdictional frameworks e.g. power of attorney</skos:scopeNote>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Legal Agent</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Agent"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#LegalEntity"/>
  </rdf:Description>
