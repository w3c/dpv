  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentUnknown">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Consent Unknown</skos:prefLabel>
    <skos:definition xml:lang="en">State where information about consent is not available or is unknown</skos:definition>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <skos:scopeNote xml:lang="en">Consent states can be unknown, for example, when information is not available, or cannot be trusted, or is known to be inaccurate</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source rdf:nodeID="b81862437284348cec0143b34a90c27af"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentRequested">
    <skos:definition xml:lang="en">State where a request for consent has been made and is awaiting a decision</skos:definition>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:scopeNote xml:lang="en">An example of this state is when a notice has been presented to the individual but they have not made a decision</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Consent Requested</skos:prefLabel>
    <dct:source rdf:nodeID="b81862437284348cec0143b34a90c27af"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing"/>
  </rdf:Description>
