  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ConsentStatusInvalidForProcessing">
    <dct:contributor rdf:nodeID="person-paul-ryan"/>
    <dct:contributor rdf:nodeID="person-harshvardhan-j-pandit"/>
    <dct:contributor rdf:nodeID="person-georg-p-krog"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:scopeNote xml:lang="en">This identifies the stages associated with consent that should not be used to process data</skos:scopeNote>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#ConsentStatus"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
