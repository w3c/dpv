      {
        "@language": "en",
        "@value": "Substitution Control"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Substitution implies that the replacement event is less risky or is more safe than the replaced event, which is distinct from elimination where no event occurs and thus there is an elimination of risk entirely"
      }
    ]
