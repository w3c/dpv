    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Control that substitutes an event with another such that the initial event no longer takes place while the substituted event takes place in place of it"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://w3id.org/dpv/risk#risk-controls-classes"
