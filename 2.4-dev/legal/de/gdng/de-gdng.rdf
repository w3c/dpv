  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#ResponsibleDataHoldingAndMediatingBody">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Zuständige datenhaltende und mittelnde Stelle</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Responsible Data-holding and Mediating Body</skos:prefLabel>
    <skos:definition xml:lang="de">Diejenige datenhaltende und -mittelnde Stelle, die für ein konkretes Forschungsvorhaben zuständig ist</skos:definition>
    <skos:definition xml:lang="en">Data holding and mediating body responsible for a specific research project</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/de/gdng#HealthDataHolder"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/de/gdng#HealthDataHolder"/>
