    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">has Data Holder</skos:prefLabel>
    <skos:definition xml:lang="en">something is related to a data holder</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/de/gdng#DataHolder"/>
    <schema:rangeIncludes rdf:resource="https://w3id.org/dpv/legal/de/gdng#DataHolder"/>
