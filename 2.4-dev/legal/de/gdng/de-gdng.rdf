  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#SecureProcessingEnvironment">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Sichere Verarbeitungsumgebung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Secure Processing Environment</skos:prefLabel>
    <skos:definition xml:lang="de">Durch die DACO konzeptionierte Maßnahme zur Verbesserung des Datenschutzes und der Datensicherheit im Rahmen der Weiterverarbeitung von Gesundheitsdaten zu Forschungszwecken und weiteren im Gemeinwohl liegenden Zwecken</skos:definition>
    <skos:definition xml:lang="en">A measure for improvement of data protection and information security in context of processing of health data for research purposes and common welfare, drafted by the DACO</skos:definition>
