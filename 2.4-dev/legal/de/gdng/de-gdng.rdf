  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#PersonalData">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Personenbezogene Daten</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Personal Data</skos:prefLabel>
    <skos:definition xml:lang="de">Personenbezogene Daten nach GDNG sind personenbezogene Daten im Sinne von Artikel 4 der DSGVO [[GDPR]]</skos:definition>
    <skos:definition xml:lang="en">Persnal data in GDNG follows the definition of personal data of Art. 4 [[GDPR]]</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 2 point 2</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
