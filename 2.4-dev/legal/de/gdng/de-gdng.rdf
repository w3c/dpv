  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#P6-3-Consent">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">[[GDNG]] § 6(3)</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Rechtsgrundlage durch Einwilligung gemäß § 6(3) GDNG</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Legal Basis by means of Consent pursuant to § 6(3) GDNG</skos:prefLabel>
    <skos:definition xml:lang="de">Weitergabe von personenbezogenen Daten an Dritte auf Grundlage einer Einwilligung der betroffenen Person nach § 6(3) GDNG, unter Berücksichtigung aller sich daraus ergebenden Rechte und Pflichten</skos:definition>
    <skos:definition xml:lang="en">Transfer of personal data to third parties based on consent of the data subject persuant to § 6(3) GDNG, taking into account all rights and obligations arising therefrom</skos:definition>
    <skos:scopeNote xml:lang="en">Compliance with all rights and obligations from § 6(3) GDNG must be ensured. These include, among others: Transfer of personal data to third parties based on consent of the data subject, in compliance with all requirements of § 6(3) GDNG for the purposes defined in § 6(1) GDNG</skos:scopeNote>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-christian-draeger"/>
    <dct:contributor rdf:nodeID="person-matthias-lobe"/>
    <dct:contributor rdf:nodeID="person-christian-henke"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-02-09</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#lawfulness-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#HealthData">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Gesundheitsdaten</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Health Data</skos:prefLabel>
    <skos:definition xml:lang="de">Gesundheitdaten nach GDNG sind Gesundheitsdaten im Sinne des Artikels 4 Nummer 15 der DSGVO [[GDPR]], einschl. Gesundheitsdaten, die zugleich Sozialdaten nach § 67 des Zehnten Buches Sozialgesetzbuch sind</skos:definition>
    <skos:definition xml:lang="en">Health data according to GDNG follows the definition of health data of Art. 4 Nr. 15 [[GDPR]], including health data that is also social data according to § 67 of Zehntes Buch Sozialgesetzbuch</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 2 point 1</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
