    <skos:prefLabel xml:lang="en">Quality Assurance</skos:prefLabel>
    <skos:definition xml:lang="de">Weiterverarbeitung von Versorgungsdaten zum Zweck der Qualitätssicherung</skos:definition>
    <skos:definition xml:lang="en">Processing of healthcare data for quality assurance</skos:definition>
