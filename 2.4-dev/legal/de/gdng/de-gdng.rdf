  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#DeletionPolicy">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">[[GDNG]] § 6(1)</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Lösch-Richtlinie</skos:prefLabel>
    <skos:prefLabel xml:lang="en">DeletionPolicy</skos:prefLabel>
    <skos:definition xml:lang="de">Nach der Rechtsgrundlage § 6(1) weitervarbeitete personenbezogene Daten müssen spätestens 30 Jahre nach Beginn der Weiterverarbeitung gelöscht werden</skos:definition>
    <skos:definition xml:lang="en">Personal data processed pursuant to the legal basis of of § 6(1) GDNG must be deleted 30 years of the beginn of processing</skos:definition>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-christian-draeger"/>
    <dct:contributor rdf:nodeID="person-matthias-lobe"/>
    <dct:contributor rdf:nodeID="person-christian-henke"/>
