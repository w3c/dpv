Class: <https://w3id.org/dpv/legal/de/gdng/owl#HealthDataAccessApplication>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "Process of application by data users for access to health data stored at health data holders. The DACO supports data users with this process and coordinates communication"@en,
        skos:definition "Prozess zur Beantragung von Zugang zu Gesundheitsdaten bei datenhaltenden Stellen durch Datennutzende, der durch die DACO unterstützt und koordiniert wird"@de,
        skos:prefLabel "Antrag auf Zugang zu Gesundheitsdaten"@de,
        skos:prefLabel "Health Data Access Application"@en
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#HealthDataAccessApplicationRecord>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "Im Rahmen der Antragsstellung des Zugangs auf Gesundheitsdaten zu erfassende und anfallende Daten, die auch im öffentlichen Antragsregister veröffentlicht werden"@de,
        skos:definition "In course of health data access applications collected and produced data that is also published in the public health data access application register"@en,
        skos:prefLabel "Gesundheitsdatenzugangsantragsdaten"@de,
        skos:prefLabel "Health Data Access Application Record"@en
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#HealthDataQuery>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "Process of identification and localization of health data by health data users for their purposes, supported by the DACO"@en,
        skos:definition "Prozess der Identifizierung und Lokalisierung von Gesundheitsdaten durch Datennutzende für ihre Zwecke, welche dazu von der DACO beraten werden"@de,
        skos:prefLabel "Identifizierung und Lokalisierung von Gesundheitsdaten"@de,
        skos:prefLabel "Indentification and Localization of Health Data"@en
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#HealthDataQueryRecord>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "Data that is collected or produced during the identification and localization of health data"@en,
        skos:definition "Die im Rahmen der Identifikation und Lokalisierung von Gesundheitsdaten zu erfassenden und anfallende Daten"@de,
        skos:prefLabel "Gesundheitsdatenanfragedaten"@de,
        skos:prefLabel "Health Data Query Record"@en
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#PublicHealthDataAccessApplicationRegister>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "A public registry of information about health data access applications, the corresponding data users, research projects and requested data, maintained by the DACO"@en,
        skos:definition "Von der DACO gepflegtes, öffentliches Register mit Informationen zu den über die DACO gestellten Anträgen auf Zugang zu Gesundheitsdaten, zu den Datennutzenden, zu den Vorhaben, für die Daten beantragt wurden, und zu deren Ergebnissen"@de,
        skos:prefLabel "Public Health Data Access Application Register"@en,
        skos:prefLabel "Öffentliches Antragsregister"@de
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#PublicMetadataCatalogue>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "An accessible catalog, driven and maintained by the DACO, in which information about existing and publicly available health data of the german health care along with the respective data holder are collected to fulfill transparency obligations"@en,
        skos:definition "Von der DACO barrierefrei zu führender und pflegender Katalog, in dem zu Transparenzzwecken Informationen über die im deutschen Gesundheitswesen vorhandenen und öffentlich zugänglichen Gesundheitsdaten und über die jeweiligen Halter dieser Daten gesammelt werden"@de,
        skos:prefLabel "Public Metadata Catalogue"@en,
        skos:prefLabel "Öffentlicher Metadaten-Katalog"@de
    
    
Class: <https://w3id.org/dpv/legal/de/gdng/owl#SecureProcessingEnvironment>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/de/gdng/owl#>,
        skos:definition "A measure for improvement of data protection and information security in context of processing of health data for research purposes and common welfare, drafted by the DACO"@en,
        skos:definition "Durch die DACO konzeptionierte Maßnahme zur Verbesserung des Datenschutzes und der Datensicherheit im Rahmen der Weiterverarbeitung von Gesundheitsdaten zu Forschungszwecken und weiteren im Gemeinwohl liegenden Zwecken"@de,
        skos:prefLabel "Secure Processing Environment"@en,
        skos:prefLabel "Sichere Verarbeitungsumgebung"@de
    
    
