    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Informationspflicht</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Duty to Inform</skos:prefLabel>
    <skos:definition xml:lang="de">Informationspflicht datenverarbeitender Gesundheitseinrichtungen</skos:definition>
    <skos:definition xml:lang="en">Duty of Health Data Processing Healthcare Facility to Inform</skos:definition>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
