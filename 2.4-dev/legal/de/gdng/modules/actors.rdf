  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#HealthDataHolder">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Datenhaltende Stelle</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Health Data Holder</skos:prefLabel>
    <skos:definition xml:lang="de">Natürliche oder juristische Personen, die berechtigt oder verpflichtet sind, Gesundheitsdaten für Forschungszwecke zu verarbeiten</skos:definition>
    <skos:definition xml:lang="en">Natural or legal person that is authorised or obligated to process health data for research purposes</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 2(3)</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#actors-classes"/>
  </rdf:Description>
