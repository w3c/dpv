  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#AuthorizedPerson">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="de">Befugte Person</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Authorized Person</skos:prefLabel>
    <skos:definition xml:lang="de">Eine natürliche Person die nach einem Rechte- und Rollenkonzept die Weiterverarbeitung von Gesundheitsdaten nach der Rechtsgrundlage § 6(1) GDNG an einer Gesundheitseinrichtung durchführt</skos:definition>
    <skos:definition xml:lang="en">A natural person that, according to a rights and rolls concept, proccesses healtcare data at a healthcare facility pursuant to the legal basis of § 6(1) GDNG</skos:definition>
    <dct:source xml:lang="en">[[GDNG]] § 6(1)</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#actors-classes"/>
  </rdf:Description>
