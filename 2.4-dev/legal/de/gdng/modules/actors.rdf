    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#actors-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#ModelProjectGenomeSequencingPlatform">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/legal/de/gdng#RegulatedHealthDataHolder"/>
    <skos:prefLabel xml:lang="de">Plattform des Modellvorhabens Genomsequenzierung</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Model Project Genome Sequencing Platform</skos:prefLabel>
    <skos:definition xml:lang="de">Plattform nach § 64e Modellvorhaben zur umfassenden Diagnostik und Therapiefindung mittels Genomsequenzierung bei seltenen und bei onkologischen Erkrankungen, Verordnungsermächtigung des Fünften Buches Sozialgesetzbuch</skos:definition>
    <skos:definition xml:lang="en">Model Project Platform pursuant to § 64e of Fünften Buches Sozialgesetzbuch</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/de/gdng#RegulatedHealthDataHolder"/>
    <dct:source xml:lang="en">[[GDNG]] § 2</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-christian-draeger"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/de/gdng#actors-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng#DataSubject">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
