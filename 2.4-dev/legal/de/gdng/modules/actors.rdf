    <skos:prefLabel xml:lang="de">Forschungsdatenzentrum Gesundheit</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Health Data Lab</skos:prefLabel>
    <skos:definition xml:lang="de">Forschungsdatenzentrum Gesundheit nach § 303d des Fünften Buches Sozialgesetzbuch</skos:definition>
    <skos:definition xml:lang="en">Health Data Lab pursuant to § 303d of Fünftes Buch Sozialgesetzbuch</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/legal/de/gdng#RegulatedHealthDataHolder"/>
    <dct:source xml:lang="en">[[GDNG]] § 2</dct:source>
