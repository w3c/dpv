    <dct:source xml:lang="en">[[GDNG]] § 3(2) point 10</dct:source>
    <skos:definition xml:lang="en">The federal state centres for cancer registry data pursuant to § 65c des Fünften Buches Sozialgesetzbuch</skos:definition>
    <skos:definition xml:lang="de">Die klinischen Krebsregister der Länder nach § 65c des Fünften Buches Sozialgesetzbuch</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#AuthorizedPerson">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:definition xml:lang="en">A natural person that, according to a rights and rolls concept, proccesses healtcare data at a healthcare facility pursuant to the legal basis of § 6(1) GDNG</skos:definition>
    <skos:definition xml:lang="de">Eine natürliche Person die nach einem Rechte- und Rollenkonzept die Weiterverarbeitung von Gesundheitsdaten nach der Rechtsgrundlage § 6(1) GDNG an einer Gesundheitseinrichtung durchführt</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="de">Befugte Person</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Authorized Person</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <dct:source xml:lang="en">[[GDNG]] § 6(1)</dct:source>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#HealthDataHolder">
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <skos:definition xml:lang="en">Natural or legal person that is authorised or obligated to process health data for research purposes</skos:definition>
    <skos:definition xml:lang="de">Natürliche oder juristische Personen, die berechtigt oder verpflichtet sind, Gesundheitsdaten für Forschungszwecke zu verarbeiten</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <skos:prefLabel xml:lang="de">Datenhaltende Stelle</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Health Data Holder</skos:prefLabel>
    <dct:source xml:lang="en">[[GDNG]] § 2(3)</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
  </rdf:Description>
