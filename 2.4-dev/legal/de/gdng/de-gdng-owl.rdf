  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#Pseudonymization">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <dct:contributor rdf:nodeID="person-christian-draeger"/>
    <dct:contributor rdf:nodeID="person-matthias-lobe"/>
    <dct:contributor rdf:nodeID="person-christian-henke"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <skos:prefLabel xml:lang="de">Pseudonymisierung von Gesundheitsdaten</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Pseudonymization of Data</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="de">Nach der Rechtsgrundlage § 6(1) GDNG weitervarbeitete personenbezogene Daten müssen pseudonymisiert werden</skos:definition>
    <skos:definition xml:lang="en">Personal data processed under § 6(1) GDNG has to be pseudonymized</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <dct:source xml:lang="en">[[GDNG]] § 6(1)</dct:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#SecureProcessingEnvironment">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <skos:definition xml:lang="de">Durch die DACO konzeptionierte Maßnahme zur Verbesserung des Datenschutzes und der Datensicherheit im Rahmen der Weiterverarbeitung von Gesundheitsdaten zu Forschungszwecken und weiteren im Gemeinwohl liegenden Zwecken</skos:definition>
    <skos:definition xml:lang="en">A measure for improvement of data protection and information security in context of processing of health data for research purposes and common welfare, drafted by the DACO</skos:definition>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-christian-henke"/>
    <dct:source xml:lang="en">[[GDNG]] § 3(2) point 9</dct:source>
    <skos:prefLabel xml:lang="en">Secure Processing Environment</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Sichere Verarbeitungsumgebung</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#P6-3-Consent">
    <skos:definition xml:lang="de">Weitergabe von personenbezogenen Daten an Dritte auf Grundlage einer Einwilligung der betroffenen Person nach § 6(3) GDNG, unter Berücksichtigung aller sich daraus ergebenden Rechte und Pflichten</skos:definition>
    <skos:definition xml:lang="en">Transfer of personal data to third parties based on consent of the data subject persuant to § 6(3) GDNG, taking into account all rights and obligations arising therefrom</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Legal Basis by means of Consent pursuant to § 6(3) GDNG</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Rechtsgrundlage durch Einwilligung gemäß § 6(3) GDNG</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor rdf:nodeID="person-christian-draeger"/>
    <dct:contributor rdf:nodeID="person-matthias-lobe"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <dct:contributor rdf:nodeID="person-christian-henke"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-02-09</dct:created>
    <dct:source xml:lang="en">[[GDNG]] § 6(3)</dct:source>
    <skos:scopeNote xml:lang="en">Compliance with all rights and obligations from § 6(3) GDNG must be ensured. These include, among others: Transfer of personal data to third parties based on consent of the data subject, in compliance with all requirements of § 6(3) GDNG for the purposes defined in § 6(1) GDNG</skos:scopeNote>
  </rdf:Description>
