  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#FederalStateCentresForCancerRegistryData">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="de">klinische Krebsregister der Länder</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Federal State Centres for Cancer Registry Data</skos:prefLabel>
