  <rdf:Description rdf:about="https://w3id.org/dpv/legal/de/gdng/owl#RegulatedHealthDataHolder">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#HealthDataHolder"/>
    <skos:prefLabel xml:lang="de">Gesetzlich geregelte datenhaltende Stelle</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Regulated Health Data Holder</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-13</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">[[GDNG]] § 2(3)</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/de/gdng/owl#"/>
    <dct:contributor rdf:nodeID="person-julian-flake"/>
    <skos:definition xml:lang="en">Data holders regulated by law. Examples are the German "Health Data Lab", the "German Centre for Cancer Registry Data" and the "Model Project Genome Sequencing"</skos:definition>
    <skos:definition xml:lang="de">Solche datenhaltenden Stellen, die gesetzlich geregelt sind, wie das Forschungsdatenzentrum Gesundheit, das Zentrum für Krebregisterdaten beim RKI und die Plattform nach §64e Abs.9 des fünften Buches Sozialgesetzbuch</skos:definition>
  </rdf:Description>
