eu-ehds-owl:A10 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.10"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to opt out from the access to their personal electronic health data registered in an EHR system through the electronic health data access services"@en ;
    skos:prefLabel "A10 Right of natural persons to opt out from primary use"@en .

eu-ehds-owl:A100 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.100"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural and legal persons to receive compensation in accordance with EU and national law in case that they have suffered material or non-material damage as a result of an infringement of the EHDS Regulation"@en ;
    skos:prefLabel "A100 Right to receive compensation"@en .

eu-ehds-owl:A16-1 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.16.1"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to identify themselves electronically using any electronic identification means which are recognised pursuant to Article 6 of Regulation (EU) No 910/2014"@en ;
    skos:prefLabel "A16-1 Right of identification management"@en .

eu-ehds-owl:A21 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.21"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural and legal persons to lodge a complaint with the competent digital health authority"@en ;
    skos:prefLabel "A21 Right to lodge a complaint with a digital health authority"@en .

eu-ehds-owl:A3 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.3"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to access personal electronic health data relating to them that belong to the priority categories referred to in Article 14 and are processed for the provision of healthcare through the electronic health data access services referred to in Article 4"@en ;
    skos:prefLabel "A3 Right of natural persons to access their personal electronic health data"@en .

eu-ehds-owl:A5 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.5"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to insert information in their EHR through electronic health data access services or applications linked to those services"@en ;
    skos:prefLabel "A5 Right of natural persons to insert information in their own EHR"@en .

eu-ehds-owl:A6 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.6"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to request online the rectification of their personal electronic health data"@en ;
    skos:prefLabel "A6 Right of natural persons to rectification"@en .

eu-ehds-owl:A68-11 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.68.11"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of health data users to access and process the electronic health data in a secure processing environment in accordance with the data permit issued to them"@en ;
    skos:prefLabel "A68-11 Right to process in a secure processing environment"@en .

eu-ehds-owl:A7 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.7"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to give access to, or to request a healthcare provider to transmit, all or part of their personal electronic health data to another healthcare provider of their choice"@en ;
    skos:prefLabel "A7 Right to data portability for natural persons"@en .

eu-ehds-owl:A71 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.71"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to opt out at any time, and without providing any reason, from the processing of personal electronic health data relating to them for secondary use"@en ;
    skos:prefLabel "A71 Right of natural persons to opt out from secondary use"@en .

eu-ehds-owl:A8 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.8"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to restrict the access of health professionals and healthcare providers to all or parts of their personal electronic health data"@en ;
    skos:prefLabel "A8 Right to restrict access"@en .

eu-ehds-owl:A9 a rdfs:Class,
        owl:Class,
        dpv-owl:Right ;
    dct:contributor _:person-beatriz-esteves ;
    dct:created "2025-10-28"^^xsd:date ;
    dct:source "EHDS Art.9"@en ;
    rdfs:isDefinedBy eu-ehds-owl: ;
    rdfs:subClassOf dpv-owl:Right ;
    sw:term_status "accepted"@en ;
    skos:definition "Right of natural persons to obtain information, including through automatic notifications, on any access to their personal electronic health data through the health professional access service obtained in the context of healthcare"@en ;
    skos:prefLabel "A9 Right to obtain information on accessing data"@en .

