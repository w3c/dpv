Datatype: rdf:langString

    
Datatype: xsd:string

    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#EHRSystem>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "A system whereby the software and/or hardware allows personal electronic health data to be collected and processed, to be used by healthcare providers when providing patient care or by patients when accessing their electronic health data"@en,
        skos:prefLabel "Electronic Health Record System"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#ElectronicHealthDataAccessService>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Technical service, such as an online portal or application, that allows data subjects to access their electronic health data or the electronic health data of those natural persons whose electronic health data they are legally authorised to access"@en,
        skos:prefLabel "Electronic Health Data Access Service"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#HealthProfessionalAccessService>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Technical service, supported by an EHR system, that enables health professionals to access data of natural persons under their care"@en,
        skos:prefLabel "Health Professional Access Service"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#InteroperabilitySoftwareForEHRSystem>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Software component of the EHR system which provides and receives personal electronic health data under a priority category for primary use established under the EHDS Regulation in the European electronic health record exchange format"@en,
        skos:prefLabel "Interoperability Software for EHR System"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#LoggingSoftwareForEHRSystem>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Software component of the EHR system which provides logging information related to access by health professionals or other individuals to priority categories of personal electronic health data"@en,
        skos:prefLabel "Logging Software for EHR System"@en
    
    
Class: <https://w3id.org/dpv/legal/eu/ehds/owl#WellnessApplication>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/legal/eu/ehds/owl#>,
        skos:definition "Software component for the processing of electronic health data, specifically for providing information on the health of natural persons, or the delivery of care for purposes other than the provision of healthcare"@en,
        skos:prefLabel "Wellness Application"@en
    
    
