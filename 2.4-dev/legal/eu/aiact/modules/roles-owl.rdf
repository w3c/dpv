    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <skos:prefLabel xml:lang="en">Authorised Representative</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">A natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by the AI Act</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority">
    <dct:source xml:lang="en">AIA(Art. 3(48))</dct:source>
