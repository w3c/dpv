eu-aiact:DeployerHumanOversightMeasure a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani,
        _:person-harshvardhan-j-pandit ;
    dct:created "2026-02-09"^^xsd:date ;
    dct:source " Art. 14 (3b)"@en,
        "AI Act"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:HumanOversight ;
    skos:definition "Human oversight measures  identified by the provider before placing the AI system on the market or putting it into service and that are appropriate to be implemented by the deployer."@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Human Oversight Measure by Deployer"@en .

eu-aiact:ProviderHumanOversightMeasure a rdfs:Class,
        skos:Concept,
        dpv:OrganisationalMeasure ;
    dct:contributor _:person-delaram-golpayegani,
        _:person-harshvardhan-j-pandit ;
    dct:created "2026-02-09"^^xsd:date ;
    dct:source " Art. 14 (3a)"@en,
        "AI Act"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:HumanOversight ;
    skos:definition "Human oversight measures built into the AI system by the provider before it is placed on the market or put into service."@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Human Oversight Measure by Provider"@en .

eu-aiact:QualityManagementSystem a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2026-02-09"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf dpv:AIGovernance,
        dpv:DataQualityManagement ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:AIGovernance,
        dpv:DataQualityManagement ;
    skos:definition "A quality management system in place in relation to an AI system that ensures compliance with the AI Act"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Quality Management System"@en .

eu-aiact:ReasonablyForeseeableMisuse a rdfs:Class,
        skos:Concept,
        dpv:RiskConcept ;
    dct:contributor _:person-arthit-suriyawongkul,
        _:person-delaram-golpayegani,
        _:person-georg-p-krog,
        _:person-harshvardhan-j-pandit ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-06-19"^^xsd:date ;
    dct:source "AIA (Art. 3(13))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "modified"@en ;
    skos:broader risk:Misuse ;
    skos:definition "The use of an AI system in a way that is not in accordance with its intended purpose, but which may result from reasonably foreseeable human behaviour or interaction with other systems, including other AI systems"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Reasonably Foreseeable Misuse"@en ;
    skos:scopeNote "Misuse should be provided through the Risk extension, along with taxonomy e.g. Misuse -> Foreseeable -> Reasonably and NonReasonably Foreseeable; and Misuse -> Unforeseeable Misuse."@en .

eu-aiact:Risk a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(2))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf dpv:Risk ;
    sw:term_status "modified"@en ;
    skos:broader dpv:Risk ;
    skos:definition "The combination of the probability of an occurrence of harm and the severity of that harm"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Risk"@en .

eu-aiact:RiskManagementSystem a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-06-12"^^xsd:date ;
    dct:source "AI Act"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf risk:RiskManagement ;
    sw:term_status "accepted"@en ;
    skos:broader risk:RiskManagement ;
    skos:definition "A risk management system is a continuous iterative process planned and run throughout the entire lifecycle of a high-risk AI system, requiring regular systematic review and updating"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Risk Management System"@en .

eu-aiact:SeriousIncident a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA (Art. 3(49))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf risk:Incident ;
    sw:term_status "modified"@en ;
    skos:broader risk:Incident ;
    skos:definition """an incident or malfunctioning of an AI system that directly or indirectly leads to any of the following:
(a) the death of a person, or serious harm to a person’s health;
(b) a serious and irreversible disruption of the management or operation of critical infrastructure;
(c) the infringement of obligations under Union law intended to protect fundamental rights;
(d) serious harm to property or the environment;"""@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Serious Incident"@en .

eu-aiact:SystemicRisk a rdfs:Class,
        skos:Concept ;
    dct:contributor _:person-delaram-golpayegani ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:modified "2025-07-30"^^xsd:date ;
    dct:source "AIA(Art.3 (65))"@en ;
    rdfs:isDefinedBy eu-aiact: ;
    rdfs:subClassOf eu-aiact:Risk ;
    sw:term_status "modified"@en ;
    skos:broader eu-aiact:Risk ;
    skos:definition "Q risk that is specific to the high-impact capabilities of general-purpose AI models, having a significant impact on the Union market due to their reach, or due to actual or reasonably foreseeable negative effects on public health, safety, public security, fundamental rights, or the society as a whole, that can be propagated at scale across the value chain"@en ;
    skos:inScheme eu-aiact:risk-classes ;
    skos:prefLabel "Systemic Risk"@en .

