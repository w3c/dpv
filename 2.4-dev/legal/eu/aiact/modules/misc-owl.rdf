  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#AILiteracy">
    <dct:contributor rdf:nodeID="person-delaram-golpayegani"/>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-02-09</dct:modified>
    <skos:scopeNote xml:lang="en">AI Literacy should take into account the subject's level of technical knowledge, experience, education and training -- these can be represented through the corresponding concepts in the PD extension</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">Skills, knowledge and understanding that allow providers, deployers and affected persons, taking into account their respective rights and obligations in the context of this Regulation, to make an informed deployment of AI systems, as well as to gain awareness about the opportunities and risks of AI and possible harm it can cause</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA (Art. 3(56))</dct:source>
    <skos:prefLabel xml:lang="en">AI Literacy</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#AILiteracy"/>
  </rdf:Description>
