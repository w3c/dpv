    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:source xml:lang="en">Art. 11, Annex IV</dct:source>
    <skos:prefLabel xml:lang="en">Technical Documentation</skos:prefLabel>
    <skos:definition xml:lang="en">Annex IV technical documentation</skos:definition>
  </rdf:Description>
