    <dct:title>EU General Data Protection Regulation (GDPR) - HTML serialiation</dct:title>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/specification"/>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DataMinimisationPrinciple">
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:source xml:lang="en">GDPR Art.5-1c</dct:source>
    <skos:definition xml:lang="en">Principle stating personal data must be processed adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed</skos:definition>
    <skos:prefLabel xml:lang="en">DataMinimisation Principle</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Principle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#LawfulnessPrinciple">
    <dct:source xml:lang="en">GDPR Art.5-1a</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#OrganisationalMeasure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Principle stating personal data must be processed processed in a lawfull manner in relation to the data subject</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:prefLabel xml:lang="en">Lawfulness Principle</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Principle"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples">
    <dct:conformsTo rdf:resource="https://www.w3.org/TR/html/"/>
    <profile:hasArtifact rdf:resource="https://w3id.org/dpv/examples"/>
    <dct:title>Examples for Data Privacy Vocabulary</dct:title>
    <dct:format rdf:resource="https://www.iana.org/assignments/media-types/text/html"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/guidance"/>
