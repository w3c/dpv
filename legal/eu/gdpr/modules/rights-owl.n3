eu-gdpr:IndirectDataCollectionNotice a rdfs:Class,
        owl:Class,
        dpv:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit" ;
    dct:created "2022-11-09"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr: ;
    rdfs:subClassOf dpv:RightFulfilmentNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "A Notice provided in fulfilment of GDPR's Art.14 regarding information to be provided where personal data are not collected from the data subject"@en ;
    skos:prefLabel "Indirect Data Collection Notice"@en .

eu-gdpr:RightsRecipientsNotice a rdfs:Class,
        owl:Class,
        dpv:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit" ;
