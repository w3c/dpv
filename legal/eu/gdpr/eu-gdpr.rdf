    <dct:contributor>Eva Schlehahn, Bud Bruegger</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#legal-basis-special-classes"/>
