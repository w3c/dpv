    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#rights-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A9-2-g">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LegalBasis"/>
    <skos:prefLabel xml:lang="en">Art 9(2-g) public interest</skos:prefLabel>
    <skos:definition xml:lang="en">substantial public interest, on the basis of Union or Member State law</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#PublicInterest"/>
    <dct:source xml:lang="en">(GDPR Art.9-2g,https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_g/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
