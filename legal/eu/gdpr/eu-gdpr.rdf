    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#OrganisationalMeasure"/>
    <skos:prefLabel xml:lang="en">DPIA Necessity Assessment</skos:prefLabel>
    <skos:definition xml:lang="en">Process that determines whether a DPIA is necessary</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#DPIA"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
