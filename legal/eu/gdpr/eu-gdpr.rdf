    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#legal-basis-special-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A14">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Right"/>
    <skos:prefLabel xml:lang="en">A14 Right to be Informed</skos:prefLabel>
    <skos:definition xml:lang="en">information to be provided where personal data is collected from other sources</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#DataSubjectRight"/>
    <dct:source xml:lang="en">(GDPR Art.14,https://eur-lex.europa.eu/eli/reg/2016/679/art_14/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
