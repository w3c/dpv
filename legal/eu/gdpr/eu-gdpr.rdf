    <skos:broader rdf:resource="https://w3id.org/dpv#LegitimateInterest"/>
    <dct:source xml:lang="en">(GDPR Art.9-2d,https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_d/oj)</dct:source>
