    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#OrganisationalMeasure"/>
    <skos:prefLabel xml:lang="en">Fairness Principle</skos:prefLabel>
    <skos:definition xml:lang="en">Principle stating personal data must be processed processed fairly in relation to the data subject</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Principle"/>
    <dct:source xml:lang="en">GDPR Art.5-1a</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-12</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/gdpr#principles-classes"/>
  </rdf:Description>
