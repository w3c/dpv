@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix eu-dga: <https://w3id.org/dpv/legal/eu/dga#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix vann: <http://purl.org/vocab/vann/> .

eu-dga:A12-e-Exchange-Approval a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 12.e,https://eur-lex.europa.eu/eli/reg/2022/868/art_12/par_e/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:LegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "Explicit request or approval of the data subject or data holder to utilise additional specific tools for the purposes of facilitating exchange of data"@en ;
    skos:prefLabel "Art 12(e) Data Exchange Approval"@en .

eu-dga:A2-6-Permission a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 2.6,https://eur-lex.europa.eu/eli/reg/2022/868/art_2/par_6/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:NonPersonalDataLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "The legal basis justfiying processing of non-personal data based on the permission of an entity"@en ;
    skos:prefLabel "Art 2(6) Permission"@en ;
    skos:scopeNote "dpv:LegalBasis needs to be divided into dpv:PersonalDataLegalBasis and dpv:NonPersonalDataLegalBasis"@en .

eu-dga:A31-2-Transfer-Agreement a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 31.2,https://eur-lex.europa.eu/eli/reg/2022/868/art_31/par_2/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:DataTransferLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "Data Transfer International Agreement"@en ;
    skos:prefLabel "Art 31(2) Data Transfer International Agreement"@en .

eu-dga:A31-3-Third-Country-Judgement a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 31.3,https://eur-lex.europa.eu/eli/reg/2022/868/art_31/par_3/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:DataTransferLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "Data Transfer Third Country Judgement"@en ;
    skos:prefLabel "Art 31(3) Data Transfer Third Country Judgement"@en .

eu-dga:A5-11-MCC a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 5.11,https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_11/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:DataTransferLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "Model Contractual Clauses"@en ;
    skos:prefLabel "Art 5(11) Model Contractual Clauses"@en .

eu-dga:A5-12-Adequacy-Decision a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 5.12,https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_12/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:DataTransferLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "Adequacy Decision permitting the transfer of data"@en ;
    skos:prefLabel "Art 5(12) Adequacy Decision"@en .

eu-dga:A5-9-Transfer-Permission a rdfs:Class,
        owl:Class,
        dpv:LegalBasis ;
    dct:source "(DGA 5.9,https://eur-lex.europa.eu/eli/reg/2022/868/art_5/par_9/oj)"@en ;
    rdfs:isDefinedBy eu-dga: ;
    rdfs:subClassOf dpv:DataTransferLegalBasis ;
    sw:term_status "accepted"@en ;
    skos:definition "The legal basis justfiying processing of non-personal data based on the permission of an entity to transfer data"@en ;
    skos:prefLabel "Art 5(9) Permission for Transfer"@en .

dpv:NonPersonalDataLegalBasis rdfs:superClassOf eu-dga:A2-6-Permission .

<https://w3id.org/dpv/legal/eu/dga> a owl:Ontology ;
    dct:conformsTo <http://www.w3.org/2002/07/owl>,
        "http://www.w3.org/2000/01/rdf-schema",
        "http://www.w3.org/2004/02/skos/core" ;
    dct:created "2023-09-20"@en ;
    dct:creator "Beatriz Esteves"@en,
        "Georg P Krog"@en,
        "Harshvardhan J. Pandit"@en ;
    dct:description "Extension to the Data Privacy Vocabulary (DPV) providing concepts for representing  information associated with EU DGA"@en ;
    dct:hasVersion <https://w3id.org/dpv/legal/eu/dga> ;
    dct:identifier "https://w3id.org/dpv/legal/eu/dga" ;
    dct:language "de" ;
    dct:license <https://www.w3.org/copyright/document-license-2023/> ;
    dct:modified "2024-01-01"@en ;
    dct:title "EU Data Governance Act (DGA)"@en ;
    vann:preferredNamespacePrefix "eu-dga" ;
    vann:preferredNamespaceUri "https://w3id.org/dpv/legal/eu/dga#" ;
    schema:version "2" .

dpv:DataTransferLegalBasis rdfs:superClassOf eu-dga:A31-2-Transfer-Agreement,
        eu-dga:A31-3-Third-Country-Judgement,
        eu-dga:A5-11-MCC,
        eu-dga:A5-12-Adequacy-Decision,
        eu-dga:A5-9-Transfer-Permission .

dpv:LegalBasis rdfs:superClassOf eu-dga:A12-e-Exchange-Approval .

