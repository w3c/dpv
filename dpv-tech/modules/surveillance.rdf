  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-tech#OvertSurveillanceTechnology">
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv/dpv-tech#SurveillanceTechnology"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-tech#SurveillanceTechnology"/>
    <skos:prefLabel xml:lang="en">Overt Surveillance Technology</skos:prefLabel>
    <skos:definition xml:lang="en">Surveillance that is overt i.e. visible or apparent or explicit</skos:definition>
    <skos:note xml:lang="en">For example, a CCTV with a notice</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-tech#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-tech#CovertSurveillanceTechnology">
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv/dpv-tech#SurveillanceTechnology"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-tech#SurveillanceTechnology"/>
    <skos:prefLabel xml:lang="en">Covert SurveillanceTechnology</skos:prefLabel>
    <skos:definition xml:lang="en">Surveillance that is covert i.e. invisible or non-apparent or implicit</skos:definition>
    <skos:note xml:lang="en">For example, a web resource that performs tracking in the background</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
