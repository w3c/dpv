    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_b/oj"/>
    <skos:prefLabel xml:lang="en">Art 49(1-b)</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <skos:definition xml:lang="en">The transfer is necessary for the performance of a contract between the data subject and controller or the implementation of pre-contractual measures taken at the data subject´s request.</skos:definition>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
