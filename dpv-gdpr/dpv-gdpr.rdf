    <skos:prefLabel xml:lang="en">A22 Right to object to automated decision making</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/gdpr#"/>
