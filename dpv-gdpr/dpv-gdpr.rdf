  <rdf:Description rdf:about="https://w3id.org/dpv/gdpr#A49-1-b">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#Contract"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#DataTransferLegalBasis"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/gdpr#"/>
