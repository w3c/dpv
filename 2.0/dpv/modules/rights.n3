    dct:title "Data Privacy Vocabulary (DPV)"@en ;
    bibo:doi "10.5281/zenodo.12505841" ;
    bibo:status "http://purl.org/ontology/bibo/status/published" ;
    vann:preferredNamespacePrefix "dpv" ;
