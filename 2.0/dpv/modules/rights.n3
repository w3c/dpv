    dct:conformsTo "http://www.w3.org/2000/01/rdf-schema",
        "http://www.w3.org/2004/02/skos/core" ;
    dct:contributor "Beatriz Esteves",
        "Georg P. Krog",
        "Harshvardhan J. Pandit",
        "Paul Ryan" ;
    dct:created "2022-08-18"@en ;
