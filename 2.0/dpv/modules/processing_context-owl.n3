    skos:definition "Indicates the level of automation involved in implementation of the specified context"@en ;
    skos:prefLabel "has automation level"@en ;
    schema:rangeIncludes dpv-owl:AutomationLevel .

dpv-owl:hasDataSource a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:DataSource ;
    dct:contributor "Georg P. Krog, Paul Ryan, Harshvardhan J. Pandit" ;
    dct:created "2020-11-04"^^xsd:date ;
    rdfs:isDefinedBy dpv-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates the source or origin of data being processed"@en ;
    skos:prefLabel "has data source"@en ;
    schema:rangeIncludes dpv-owl:DataSource .

dpv-owl:hasEntityInvolvement a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:EntityInvolvement ;
    dct:created "2024-05-11"^^xsd:date ;
    rdfs:isDefinedBy dpv-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates involvement of an entity in specified context"@en ;
    skos:prefLabel "has entity involvement"@en ;
    schema:rangeIncludes dpv-owl:EntityInvolvement .

dpv-owl:hasHumanInvolvement a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:HumanInvolvement ;
