    rdfs:isDefinedBy dpv-owl: ;
    rdfs:subPropertyOf dpv-owl:hasEntity ;
    sw:term_status "accepted"@en ;
    skos:definition "indicates the entity is actively involved in specified context"@en ;
    skos:prefLabel "has active entity"@en ;
    schema:rangeIncludes dpv-owl:EntityActiveInvolvement .

dpv-owl:hasAlgorithmicLogic a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:AlgorithmicLogic ;
