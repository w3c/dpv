    skos:prefLabel "has algorithmic logic"@en ;
    schema:rangeIncludes dpv-owl:AlgorithmicLogic .

dpv-owl:hasAutomationLevel a rdf:Property,
        owl:ObjectProperty ;
    dcam:rangeIncludes dpv-owl:AutomationLevel ;
