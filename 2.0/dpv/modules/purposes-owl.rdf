  <rdf:Description rdf:about="https://w3id.org/dpv/owl#Advertising">
    <skos:prefLabel xml:lang="en">Advertising</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdf:type rdf:resource="https://w3id.org/dpv/owl#Purpose"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:scopeNote xml:lang="en">Advertising is a subset of Marketing. Advertising by itself does not indicate 'personalisation' i.e. personalised ads.</skos:scopeNote>
    <skos:definition xml:lang="en">Purposes associated with conducting advertising i.e. process or artefact used to call attention to a product, service, etc. through announcements, notices, or other forms of communication</skos:definition>
    <dct:contributor>Georg P. Krog, Harshvardhan J. Pandit, Beatriz Esteves</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Marketing"/>
  </rdf:Description>
