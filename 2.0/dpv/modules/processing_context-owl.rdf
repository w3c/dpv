    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">Involvement where entity cannot reverse input of specified context</skos:definition>
    <skos:prefLabel xml:lang="en">Cannot Reverse Process Input</skos:prefLabel>
    <dct:contributor>Harshvardhan J. Pandit, Delaram Golpayegani, Steve Hickman</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#EntityNonPermissiveInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#ChallengingProcess">
