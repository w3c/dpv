    skos:prefLabel "has function"@en ;
    schema:domainIncludes dpv-owl:Technology ;
    schema:rangeIncludes tech-owl:Function .

tech-owl:hasIntendedUse a rdf:Property,
        owl:ObjectProperty ;
    dcam:domainIncludes dpv-owl:Technology ;
    dcam:rangeIncludes tech-owl:IntendedUse ;
    dct:contributor "Harshvardhan J. Pandit" ;
