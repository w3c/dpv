tech-owl:hasProvisionMethod a rdf:Property,
        owl:ObjectProperty ;
    dcam:domainIncludes dpv-owl:Technology ;
    dcam:rangeIncludes tech-owl:ProvisionMethod ;
    dct:contributor "Harshvardhan J. Pandit" ;
