    sw:term_status "accepted"@en ;
    skos:definition "Specifies the location of technology deployment and use"@en ;
    skos:prefLabel "has deployment location"@en ;
    schema:domainIncludes dpv-owl:Technology ;
    schema:rangeIncludes tech-owl:DeploymentLocation .

tech-owl:hasDocumentation a rdf:Property,
        owl:ObjectProperty ;
    dcam:domainIncludes dpv-owl:Technology ;
    dcam:rangeIncludes tech-owl:Documentation ;
    dct:contributor "Harshvardhan J. Pandit" ;
    dct:created "2024-06-29"^^xsd:date ;
    rdfs:isDefinedBy tech-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates documentation associated with technology"@en ;
    skos:prefLabel "has documentation"@en ;
    schema:domainIncludes dpv-owl:Technology ;
    schema:rangeIncludes tech-owl:Documentation .

tech-owl:hasFunction a rdf:Property,
        owl:ObjectProperty ;
    dcam:domainIncludes dpv-owl:Technology ;
    dcam:rangeIncludes tech-owl:Function ;
