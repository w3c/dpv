    rdfs:isDefinedBy tech-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates an actor associated with technology"@en ;
    skos:prefLabel "has actor"@en ;
    schema:domainIncludes dpv-owl:Technology ;
    schema:rangeIncludes tech-owl:TechnologyActor .

