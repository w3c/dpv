eu-aiact-owl:AIDeployer a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(4))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIOperator,
        tech-owl:Deployer ;
    sw:term_status "accepted"@en ;
    skos:definition "Any natural or legal person, public authority, agency or other body using an AI system under its authority except where the AI system is used in the course of a personal non-professional activity"@en ;
    skos:prefLabel "AI Deployer"@en .

eu-aiact-owl:AIDistributor a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(7))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIOperator,
        tech-owl:Distributor ;
    sw:term_status "accepted"@en ;
    skos:definition "Any natural or legal person in the supply chain, other than the provider or the importer, that makes an AI system available on the Union market"@en ;
    skos:prefLabel "AI Distributor"@en .

eu-aiact-owl:AIImporter a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(6))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIOperator,
        tech-owl:Importer ;
    sw:term_status "accepted"@en ;
    skos:definition "Any natural or legal person located or established in the Union that places on the market an AI system that bears the name or trademark of a natural or legal person established outside the Union"@en ;
    skos:prefLabel "AI Importer"@en .

eu-aiact-owl:AILiteracy a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bh))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:AILiteracy ;
    sw:term_status "accepted"@en ;
    skos:definition "Skills, knowledge and understanding that allows providers, users and affected persons, taking into account their respective rights and obligations in the context of this Regulation, to make an informed deployment of AI systems, as well as to gain awareness about the opportunities and risks of AI and possible harm it can cause."@en ;
    skos:prefLabel "AI Literacy"@en .

eu-aiact-owl:AIOffice a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(42))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Authority ;
    sw:term_status "accepted"@en ;
    skos:definition "Commission’s function of contributing to the implementation, monitoring and supervision of AI systems and AI governance. References in this Regulation to the Artificial Intelligence office shall be understood as references to the Commission."@en ;
    skos:prefLabel "Artificial Intelligence Office"@en .

eu-aiact-owl:AIOperator a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(8))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Operator ;
    sw:term_status "accepted"@en ;
    skos:definition "The provider, the product manufacturer, the deployer, the authorised representative, the importer or the distributor;"@en ;
    skos:prefLabel "AI Operator"@en .

eu-aiact-owl:AIProductManufacturer a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Manufacturer ;
    sw:term_status "accepted"@en ;
    skos:definition "Entity that manufactures an AI Product"@en ;
    skos:prefLabel "AI Product Manufacturer"@en .

eu-aiact-owl:AIProvider a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(2))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIOperator,
        tech-owl:Provider ;
    sw:term_status "accepted"@en ;
    skos:definition "Natural or legal person, public authority, agency or other body that develops an AI system or a general purpose AI model or that has an AI system or a general purpose AI model developed and places them on the market or puts the system into service under its own name or trademark, whether for payment or free of charge"@en ;
    skos:prefLabel "AI Provider"@en .

eu-aiact-owl:AIRegulatorySandbox a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bg))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:RegulatorySandbox ;
    sw:term_status "accepted"@en ;
    skos:definition "A concrete and controlled framework set up by a competent authority which offers providers or prospective providers of AI systems the possibility to develop, train, validate and test, where appropriate in real world conditions, an innovative AI system, pursuant to a sandbox plan for a limited time under regulatory supervision."@en ;
    skos:prefLabel "AI Regulatory Sandbox"@en .

eu-aiact-owl:AISystem a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(1))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:AISystem ;
    sw:term_status "accepted"@en ;
    skos:definition "A machine-based system designed to operate with varying levels of autonomy and that may exhibit adaptiveness after deployment and that, for explicit or implicit objectives, infers, from the input it receives, how to generate outputs such as predictions, content, recommendations, or decisions that can influence physical or virtual environments"@en ;
    skos:prefLabel "AI System"@en ;
    skos:scopeNote "Note All references to the AI Act referred to the provisional agreement published in Feb 2024"@en .

eu-aiact-owl:AISystemPerformance a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(18))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    sw:term_status "accepted"@en ;
    skos:definition "The ability of an AI system to achieve its intended purpose"@en ;
    skos:prefLabel "AI System Performance"@en .

eu-aiact-owl:AuthorisedRepresentative a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(5))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIOperator,
        dpv-owl:Representative ;
    sw:term_status "accepted"@en ;
    skos:definition "Any natural or legal person located or established in the Union who has received and accepted a written mandate from a provider of an AI system or a general-purpose AI model to, respectively, perform and carry out on its behalf the obligations and procedures established by this Regulation"@en ;
    skos:prefLabel "Authorised Representative"@en .

eu-aiact-owl:AvailableOnEUMarket a rdfs:Class,
        owl:Class,
        eu-aiact-owl:MarketAvailabilityStatus ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-05-17"^^xsd:date ;
    dct:source "AIA (Art. 3(10))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AvailableOnMarket ;
    sw:term_status "accepted"@en ;
    skos:definition "Status associated with supply of an AI system or a general purpose AI model for distribution or use on the Union market in the course of a commercial activity, whether in return for payment or free of charge"@en ;
    skos:prefLabel "Available on EU Market"@en .

eu-aiact-owl:AvailableOnMarket a rdfs:Class,
        owl:Class,
        eu-aiact-owl:MarketAvailabilityStatus ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-05-17"^^xsd:date ;
    dct:source "AIA (Art. 3(10))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:MarketAvailabilityStatus ;
    sw:term_status "accepted"@en ;
    skos:definition "Status associated with supply of an AI system or a general purpose AI model for distribution or use on the market in the course of a commercial activity, whether in return for payment or free of charge"@en ;
    skos:prefLabel "Available on Market"@en .

eu-aiact-owl:AvailableOnNonEUMarket a rdfs:Class,
        owl:Class,
        eu-aiact-owl:MarketAvailabilityStatus ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-05-17"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AvailableOnMarket ;
    sw:term_status "accepted"@en ;
    skos:definition "Status associated with supply of an AI system or a general purpose AI model for distribution or use on any market except the Union market in the course of a commercial activity, whether in return for payment or free of charge"@en ;
    skos:prefLabel "Available on Non-EU Market"@en .

eu-aiact-owl:Biometric a rdfs:Class,
        owl:Class,
        dpv-owl:PersonalData ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(33))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf pd-owl:Biometric ;
    sw:term_status "accepted"@en ;
    skos:definition "Personal data resulting from specific technical processing relating to the physical, physiological or behavioural characteristics of a natural person, such as facial images or dactyloscopic data"@en ;
    skos:prefLabel "Biometric"@en .

eu-aiact-owl:BiometricCategorisation a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "based on AIA(Art. 3(35))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "accepted"@en ;
    skos:definition "Assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons"@en ;
    skos:prefLabel "Biometric Categorisation"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:BiometricCategorisationSystem a rdfs:Class,
        owl:Class,
        eu-aiact-owl:AISystem ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(35))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AISystem ;
    sw:term_status "accepted"@en ;
    skos:definition "An AI system for the purpose of assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons"@en ;
    skos:prefLabel "Biometric Categorisation System"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:BiometricIdentification a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(33a))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "accepted"@en ;
    skos:definition "The automated recognition of physical, physiological, behavioural, and psychological human features for the purpose of establishing an individual’s identity by comparing biometric data of that individual to stored biometric data of individuals in a database"@en ;
    skos:prefLabel "Biometric Identification"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:BiometricIdentityVerification a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(33c))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Purpose ;
    sw:term_status "accepted"@en ;
    skos:definition "The automated verification of the identity of natural persons by comparing biometric data of an individual to previously provided biometric data (one-to-one verification, including authentication)."@en ;
    skos:prefLabel "Biometric Identity Verification"@en ;
    skos:related pd-owl:Biometric ;
    skos:scopeNote "Parent could be dpv:IdentityVerification"@en .

eu-aiact-owl:CEMarking a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(24))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:CertificationSeal ;
    sw:term_status "accepted"@en ;
    skos:definition "A marking by which a provider indicates that an AI system is in conformity with the requirements set out in Title III, Chapter 2 of this Regulation and other applicable Union legislation harmonising the conditions for the marketing of products (‘Union harmonisation legislation’) providing for its affixing"@en ;
    skos:prefLabel "CE Marking"@en .

eu-aiact-owl:CommonSpecification a rdfs:Class,
        owl:Class,
        tech-owl:Specification ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(28))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Specification ;
    sw:term_status "accepted"@en ;
    skos:definition "A set of technical specifications, as defined in point 4 of Article 2 of Regulation (EU) No 1025/2012 providing means to comply with certain requirements established under the AI Act"@en ;
    skos:prefLabel "Common Specification"@en .

eu-aiact-owl:ConformityAssessment a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(20))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "Assessment of the AI system performance with regards to achieving its intended purpose"@en ;
    skos:prefLabel "Conformity Assessment"@en .

eu-aiact-owl:ConformityAssessmentBody a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(21))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Auditor ;
    sw:term_status "accepted"@en ;
    skos:definition "A body that performs third-party conformity assessment activities, including testing, certification and inspection"@en ;
    skos:prefLabel "Conformity Assessment Body"@en .

eu-aiact-owl:CriticalInfrastructure a rdfs:Class,
        owl:Class,
        dpv-owl:Sector ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(44h))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Sector ;
    sw:term_status "accepted"@en ;
    skos:definition "An asset, a facility, equipment, a network or a system, or a part of thereof, which is necessary for the provision of an essential service within the meaning of Article 2(4) of Directive (EU) 2022/2557"@en ;
    skos:prefLabel "Critical Infrastructure"@en .

eu-aiact-owl:DeepFake a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bl))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "accepted"@en ;
    skos:definition "Capability to generate or manipulate image, audio or video content that resembles existing persons, objects, places or other entities or events and would falsely appear to a person to be authentic or truthful"@en ;
    skos:prefLabel "Deep Fake"@en .

eu-aiact-owl:DownstreamAIProvider a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(44g))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AIProvider ;
    sw:term_status "accepted"@en ;
    skos:definition "A provider of an AI system, including a general-purpose AI system, which integrates an AI model, regardless of whether the model is provided by themselves and vertically integrated or provided by another entity based on contractual relations."@en ;
    skos:prefLabel "Downstream AI Provider"@en .

eu-aiact-owl:EmotionRecognition a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "accepted"@en ;
    skos:definition "Capability of a technology to identify or infer emotions or intentions of natural persons [on the basis of their biometric data]"@en ;
    skos:prefLabel "Emotion Recognition"@en ;
    skos:related pd-owl:Biometric .

eu-aiact-owl:EmotionRecognitionSystem a rdfs:Class,
        owl:Class,
        eu-aiact-owl:AISystem ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(34))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AISystem ;
    sw:term_status "accepted"@en ;
    skos:definition "An AI system for the purpose of identifying or inferring emotions or intentions of natural persons on the basis of their biometric data."@en ;
    skos:prefLabel "Emotion Recognition System"@en ;
    skos:related eu-aiact-owl:EmotionRecognition,
        pd-owl:Biometric .

eu-aiact-owl:FRIA a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "Art. 29a"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "An assessment undertaken to evaluate how the system might impact fundamental rights"@en ;
    skos:prefLabel "Fundamental Rights Impact Assessment"@en .

eu-aiact-owl:GPAIModel a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(44b))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf ai-owl:Model ;
    sw:term_status "accepted"@en ;
    skos:definition "An AI model, including when trained with a large amount of data using self-supervision at scale, that displays significant generality and is capable to competently perform a wide range of distinct tasks regardless of the way the model is placed on the market and that can be integrated into a variety of downstream systems or applications. This does not cover AI models that are used before release on the market for research, development and prototyping activities"@en ;
    skos:prefLabel "General Purpose AI Model"@en .

eu-aiact-owl:GPAISystem a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art.3 (44e))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf eu-aiact-owl:AISystem ;
    sw:term_status "accepted"@en ;
    skos:definition "An AI system which is based on a general-purpose AI model, that has the capability to serve a variety of purposes, both for direct use as well as for integration in other AI systems"@en ;
    skos:prefLabel "General Purpose AI System"@en .

eu-aiact-owl:HighImpactCapabilityGPAIModel a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA(Art. 3(44c))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Capability ;
    sw:term_status "accepted"@en ;
    skos:definition "‘high-impact capabilities’ in general purpose AI models means capabilities that match or exceed the capabilities recorded in the most  advanced general purpose AI models."@en ;
    skos:prefLabel "High-Impact Capability in General Purpose AI Models"@en .

eu-aiact-owl:HighRiskAIAssessment a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "Art. 9 (2b)"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:Assessment ;
    sw:term_status "accepted"@en ;
    skos:definition "An assessment undertaken to determine whether the AI system is classified as high-risk"@en ;
    skos:prefLabel "High Risk AI Assessment"@en .

eu-aiact-owl:InformedConsent a rdfs:Class,
        owl:Class,
        dpv-owl:LegalBasis ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bk))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf dpv-owl:InformedConsent ;
    sw:term_status "accepted"@en ;
    skos:definition "Informed consent means a (real world test) subject’s freely given, specific, unambiguous and voluntary expression of his or her willingness to participate in a particular testing in real world conditions, after having been informed of all aspects of the testing that are relevant to the subject’s decision to participate;"@en ;
    skos:prefLabel "Informed Consent"@en .

eu-aiact-owl:InstructionForUse a rdfs:Class,
        owl:Class,
        tech-owl:Instructions ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(15))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
    rdfs:subClassOf tech-owl:Instructions ;
    sw:term_status "accepted"@en ;
    skos:definition "The information provided by the provider to inform the user of in particular an AI system’s intended purpose and proper use"@en ;
    skos:prefLabel "Instruction For Use"@en .

eu-aiact-owl:IntendedPurpose a rdfs:Class,
        owl:Class ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(12))"@en ;
    rdfs:isDefinedBy eu-aiact-owl: ;
