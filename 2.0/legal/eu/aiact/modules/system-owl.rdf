  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#BiometricCategorisationSystem">
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Biometric Categorisation System</skos:prefLabel>
    <skos:related rdf:resource="https://w3id.org/dpv/pd/owl#Biometric"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(35))</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:definition xml:lang="en">An AI system for the purpose of assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#GPAISystem">
    <dct:source xml:lang="en">AIA(Art.3 (44e))</dct:source>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">General Purpose AI System</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#AISystem"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">An AI system which is based on a general-purpose AI model, that has the capability to serve a variety of purposes, both for direct use as well as for integration in other AI systems</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#serialisation-rdf">
    <dct:title>EU AI Act - RDF/XML serialiation</dct:title>
    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/ResourceDescriptor"/>
    <profile:hasRole rdf:resource="http://www.w3.org/ns/dx/prof/role/vocabulary"/>
    <dct:conformsTo rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
