  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#BiometricCategorisationSystem">
    <skos:prefLabel xml:lang="en">Biometric Categorisation System</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <skos:definition xml:lang="en">An AI system for the purpose of assigning natural persons to specific categories on the basis of their biometric data unless ancillary to another commercial service and strictly necessary for objective technical reasons</skos:definition>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA(Art. 3(35))</dct:source>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/pd/owl#Biometric"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#EmotionRecognitionSystem">
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source xml:lang="en">AIA(Art. 3(34))</dct:source>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#EmotionRecognition"/>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/pd/owl#Biometric"/>
    <skos:definition xml:lang="en">An AI system for the purpose of identifying or inferring emotions or intentions of natural persons on the basis of their biometric data.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <skos:prefLabel xml:lang="en">Emotion Recognition System</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#GPAISystem">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:source xml:lang="en">AIA(Art.3 (44e))</dct:source>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">An AI system which is based on a general-purpose AI model, that has the capability to serve a variety of purposes, both for direct use as well as for integration in other AI systems</skos:definition>
    <skos:prefLabel xml:lang="en">General Purpose AI System</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#RemoteBiometricIdentificationSystem">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:related rdf:resource="https://w3id.org/dpv/2.0/pd/owl#Biometric"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
    <skos:definition xml:lang="en">An AI system for the purpose of identifying natural persons, without their active involvement, typically at a distance through the comparison of a person’s biometric data with the biometric data contained in a reference database.</skos:definition>
    <skos:prefLabel xml:lang="en">Remote Biometric Identification System</skos:prefLabel>
    <dct:source xml:lang="en">AIAArt. 3(36))</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#AISystem">
    <skos:prefLabel xml:lang="en">AI System</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <skos:definition xml:lang="en">A machine-based system designed to operate with varying levels of autonomy and that may exhibit adaptiveness after deployment and that, for explicit or implicit objectives, infers, from the input it receives, how to generate outputs such as predictions, content, recommendations, or decisions that can influence physical or virtual environments</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/2.0/ai/owl#AISystem"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source xml:lang="en">AIA (Art. 3(1))</dct:source>
    <skos:scopeNote xml:lang="en">Note All references to the AI Act referred to the provisional agreement published in Feb 2024</skos:scopeNote>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/2.0/legal/eu/aiact/owl#"/>
  </rdf:Description>
