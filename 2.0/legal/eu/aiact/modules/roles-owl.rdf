  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/aiact/owl#NationalCompetentAuthority">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-10</dct:created>
    <dct:source xml:lang="en">AIA(Art. 3(42))</dct:source>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#Authority"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/aiact/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">National Competent Authority</skos:prefLabel>
    <skos:definition xml:lang="en">The notifying authority and the market surveillance authority. As regards AI systems put into service or used by EU institutions, agencies, offices and bodies, any reference to national competent authorities or market surveillance authorities in this Regulation shall be understood as referring to the European Data Protection Supervisor.</skos:definition>
    <dct:contributor>Delaram Golpayegani</dct:contributor>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
