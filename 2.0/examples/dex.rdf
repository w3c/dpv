    <dct:format>text/turtle</dct:format>
    <dct:subject rdf:resource="https://w3id.org/dpv#Subsidiary"/>
    <dct:subject rdf:resource="https://w3id.org/dpv#hasSubsidiary"/>
    <dct:subject rdf:resource="https://w3id.org/dpv#isSubsidiaryFor"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-10</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples#E0049">
    <rdf:type rdf:resource="https://w3id.org/dpv/examples#Example"/>
    <dct:title>Indicating data volume, geo-location coverage, data subject scale, and a processing scale</dct:title>
    <dct:description>This example shows how data volume, data subject scale, and geographic scale can be indicated along with optional information about the exact values involved. It also shows how a qualified scale can be provided as 'processing scale' based on some criteria (not described here).</dct:description>
    <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">E0049.ttl</dct:source>
    <dct:format>text/turtle</dct:format>
