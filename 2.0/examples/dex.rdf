    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-13</dct:created>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples#E0051">
    <rdf:type rdf:resource="https://w3id.org/dpv/examples#Example"/>
    <dct:title>Specifying frequency</dct:title>
    <dct:description>This example shows the various ways in which frequency can be expressed, including combining frequency with duration to express complex information such as once per day for 6 months</dct:description>
    <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">E0051.ttl</dct:source>
