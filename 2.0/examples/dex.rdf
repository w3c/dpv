    <dct:subject rdf:resource="https://w3id.org/dpv#GeographicCoverage"/>
    <dct:subject rdf:resource="https://w3id.org/dpv#DataVolume"/>
    <dct:subject rdf:resource="https://w3id.org/dpv#DataSubjectScale"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-11</dct:created>
