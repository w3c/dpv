  <rdf:Description rdf:about="https://w3id.org/dpv/examples#E0019">
    <rdf:type rdf:resource="https://w3id.org/dpv/examples#Example"/>
    <dct:title>Indicating Entity Information, including DPO and Representatives</dct:title>
    <dct:description>This example shows how information about representatives can be associated with an entity</dct:description>
    <dct:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">E0019.ttl</dct:source>
    <dct:format>text/turtle</dct:format>
