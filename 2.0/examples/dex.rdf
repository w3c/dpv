  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/examples#E0038">
    <rdf:type rdf:resource="https://w3id.org/dpv/examples#Example"/>
    <dct:title>Indicating subsidiaries of an organisation</dct:title>
    <dct:description>This example shows the existence of two subsidiaries associated with an organisation and their locations</dct:description>
