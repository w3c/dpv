    <dct:source rdf:resource="https://w3id.org/GConsent"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Julian Flake</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <skos:definition xml:lang="en">State where a request for consent has been deferred without a decision</skos:definition>
    <skos:prefLabel xml:lang="en">Consent Request Deferred</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#ConsentStatusInvalidForProcessing"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-22</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#ConsentStatusInvalidForProcessing"/>
    <skos:note xml:lang="en">An example of this state is when the individual closes or dismisses a notice without making a decision. This state is intended for making the distinction between a notice being provided (as a consent request) and the individual interacting with the notice without making a decision - where the 'ignoring of a notice' is taken as consent being neither given nor refused</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#ExpressedConsent">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <skos:note xml:lang="en">Expressed consent requires the individual take a specific and unambigious action that directly indicates their consent. This action may be a part of other processes such as setting preferences, or agreeing to a contract, or other matters not relating to consent. An example of expressed consent is interacting with a checkbox within a dashboard or clicking a button on a web form</skos:note>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Julian Flake</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <skos:prefLabel xml:lang="en">Expressed Consent</skos:prefLabel>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-21</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#InformedConsent"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#InformedConsent"/>
    <skos:definition xml:lang="en">Consent that is expressed through an action intended to convey a consenting decision</skos:definition>
  </rdf:Description>
