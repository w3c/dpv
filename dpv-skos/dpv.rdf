  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#SensitivePersonalData">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#PersonalData"/>
