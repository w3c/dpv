    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#ProcessingContext"/>
    <skos:definition xml:lang="en">Contextual information about the degree of automation and human involvement associated with Processing</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
