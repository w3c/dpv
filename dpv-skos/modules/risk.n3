    rdfs:isDefinedBy dpvs: ;
    rdfs:range dpvs:Risk ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates applicability of Risk for this concept"@en ;
    skos:prefLabel "has risk"@en .

